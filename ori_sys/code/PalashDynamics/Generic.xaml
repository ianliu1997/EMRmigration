                            <ContentPresenter Content="To Time" Grid.Row="2" Grid.Column="2" 
                                          VerticalAlignment="Center" HorizontalAlignment="Center" />
                            <Rectangle Fill="#FFC9CACA" VerticalAlignment="Stretch" Width="1"  
                                   Visibility="Visible" Grid.Row="2" Grid.Column="3" />
                            <ContentPresenter Content="Cabin" Grid.Row="2" Grid.Column="4" 
                                          VerticalAlignment="Center" HorizontalAlignment="Center" />

