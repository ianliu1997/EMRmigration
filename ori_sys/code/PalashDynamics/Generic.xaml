    <LinearGradientBrush x:Key="orange gradient 1" StartPoint="0.29067,0.571309" EndPoint="0.678223,0.571309">
        <GradientStop Color="#FFFFC100" Offset="0"/>
        <GradientStop Color="#FFFF9200" Offset="1"/>
