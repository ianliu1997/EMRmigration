                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Successfull" Grid.Row="1" Grid.Column="0" Margin="4" ></TextBlock>
                                <StackPanel Orientation="Horizontal"  Grid.Row="1" Grid.Column="1" VerticalAlignment="Center">
                                    <RadioButton x:Name="rdbIUISuccessfullY" Content="Yes" Margin="4"></RadioButton>
                                    <RadioButton x:Name="rdbIUISuccessfullN" Content="No" Margin="4"  IsChecked="True"></RadioButton>
                                </StackPanel>

                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Place" Grid.Row="1" Grid.Column="2" Margin="4" ></TextBlock>
                                <TextBox Grid.Row="1" Grid.Column="3" Margin="4" Text="{Binding IUIPlace, Mode=TwoWay}" x:Name="txtIUIPlace"  HorizontalAlignment="Stretch" TabIndex="0"></TextBox>


                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Previous IVF" Grid.Row="2" Grid.Column="0" Margin="4" ></TextBlock>
                                <StackPanel Orientation="Horizontal"  Grid.Row="2" Grid.Column="1" VerticalAlignment="Center">
                                    <RadioButton x:Name="rdbPreviousIVFY" Content="Yes" Margin="4"></RadioButton>
                                    <RadioButton x:Name="rdbPreviousIVFN" Content="No" Margin="4"  IsChecked="True"></RadioButton>
                                </StackPanel>

                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="No Of Times" Grid.Row="2" Grid.Column="2" Margin="4" ></TextBlock>
                                                <TextBox Grid.Row="2" Grid.Column="3" Margin="4" Text="{Binding IVFNoOfTimes, Mode=TwoWay}" x:Name="txtIVFNoOfTimes" TextAlignment="Right"  HorizontalAlignment="Stretch" TabIndex="0"></TextBox>


                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Successfull" Grid.Row="3" Grid.Column="0" Margin="4" ></TextBlock>
                                <StackPanel Orientation="Horizontal"  Grid.Row="3" Grid.Column="1" VerticalAlignment="Center">
                                    <RadioButton x:Name="rdbIVFSuccessfullY" Content="Yes" Margin="4"></RadioButton>
                                    <RadioButton x:Name="rdbIVFSuccessfullN" Content="No" Margin="4"  IsChecked="True"></RadioButton>
                                </StackPanel>

                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Place" Grid.Row="3" Grid.Column="2" Margin="4" ></TextBlock>
                                <TextBox Grid.Row="3" Grid.Column="3" Margin="4" Text="{Binding IVFPlace, Mode=TwoWay}" x:Name="txtIVFPlace"  HorizontalAlignment="Stretch" TabIndex="0"></TextBox>

                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Special Notes if any" Grid.Row="4" Grid.Column="0" Margin="4" ></TextBlock>
                                <TextBox Grid.Row="4" Grid.Column="1"  Margin="4" Text="{Binding SpecialNotes, Mode=TwoWay}" x:Name="txtSpecialNotes"  HorizontalAlignment="Stretch" TabIndex="0"></TextBox>

                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Illness" Grid.Row="4" Grid.Column="2" Margin="4" ></TextBlock>
                                <TextBox Grid.Row="4" Grid.Column="3"  Margin="4" Text="{Binding Illness, Mode=TwoWay}" x:Name="txtIllness"  HorizontalAlignment="Stretch" TabIndex="0"></TextBox>

                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Allergies" Grid.Row="5" Grid.Column="0" Margin="4" ></TextBlock>
                                <TextBox Grid.Row="5" Grid.Column="1"  Margin="4" Text="{Binding Allergies, Mode=TwoWay}" x:Name="txtAllergies"  HorizontalAlignment="Stretch" TabIndex="0"></TextBox>

                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Family / Social History" Grid.Row="5" Grid.Column="2" Margin="4" ></TextBlock>
                                <TextBox Grid.Row="5" Grid.Column="3"  Margin="4" Text="{Binding FamilySocialHistory, Mode=TwoWay}" x:Name="txtFamilySocialHistory"  HorizontalAlignment="Stretch" TabIndex="0"></TextBox>


                            </Grid>
                        </Grp:GroupBox>
                    </controlsToolkit:DockPanel>

                    <controlsToolkit:DockPanel x:Name="docBottom" controlsToolkit:DockPanel.Dock="Bottom" >
                        <Grp:GroupBox x:Name="grpOtherRelaventHistory" IsTabStop="False" Header="Other Relavent History" Grid.Row="2" Margin="4" >
                            <Grid>


                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                    <ColumnDefinition ></ColumnDefinition>

                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                </Grid.RowDefinitions>

                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Obst. History (Complications if any)" Grid.Row="0" Grid.Column="0" Margin="4" ></TextBlock>
                                <TextBox Grid.Row="0" Grid.Column="1" Margin="4" Text="{Binding ObstHistoryComplications, Mode=TwoWay}" x:Name="txtObstHistoryComplications"  HorizontalAlignment="Stretch" TabIndex="0" AcceptsReturn="True" VerticalScrollBarVisibility="Visible" TextWrapping="Wrap"></TextBox>

                                <TextBlock Grid.Row="1"  VerticalAlignment="Center" HorizontalAlignment="Right" Text="Surgeries" Grid.Column="0" Margin="4" ></TextBlock>
                                <TextBox Grid.Row="1" Grid.Column="1" Margin="4" Text="{Binding Surgeries, Mode=TwoWay}" x:Name="txtSurgeries"  HorizontalAlignment="Stretch" TabIndex="0" AcceptsReturn="True" VerticalScrollBarVisibility="Visible" TextWrapping="Wrap"></TextBox>

                            </Grid>
