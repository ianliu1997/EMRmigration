                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Regular" Grid.Row="0" Grid.Column="2" Margin="4" ></TextBlock>
                                <!--<TextBox Grid.Row="0" Grid.Column="3" Margin="4" Text="{Binding HairColor, Mode=TwoWay}" x:Name="txtHairColor"  HorizontalAlignment="Stretch" TabIndex="0"></TextBox>-->
                                <!--<CheckBox x:Name="chkRegular"  Margin="4" IsChecked="{Binding Regular}" VerticalAlignment="Center" ></CheckBox>-->
                                <StackPanel Orientation="Horizontal"  Grid.Row="0" Grid.Column="3" VerticalAlignment="Center">
                                    <RadioButton x:Name="rdbRegularY" Content="Yes" Margin="4" IsChecked="True"></RadioButton>
                                    <RadioButton x:Name="rdbRegularN" Content="No" Margin="4" ></RadioButton>
                                </StackPanel>

                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Length" Grid.Row="1" Grid.Column="0" Margin="4" ></TextBlock>
                                                <TextBox Grid.Row="1" Grid.Column="1" Margin="4" Text="{Binding Length, Mode=TwoWay}" x:Name="txtLength" TextAlignment="Right"  HorizontalAlignment="Stretch" TabIndex="0"></TextBox>

                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Duration Of Period" Grid.Row="1" Grid.Column="2" Margin="4" ></TextBlock>
                                                <TextBox Grid.Row="1" Grid.Column="3" Margin="4" Text="{Binding DurationOfPeriod, Mode=TwoWay}" x:Name="txtDurationOfPeriod" TextAlignment="Right"  HorizontalAlignment="Stretch" TabIndex="0"></TextBox>

                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Blood Loss" Grid.Row="2" Grid.Column="0" Margin="4" ></TextBlock>
                                <!--<TextBox Grid.Row="2" Grid.Column="1" Margin="4" Text="{Binding GeneralGenitalExam, Mode=TwoWay}" x:Name="txtGeneralGenitalExam"  HorizontalAlignment="Stretch" TabIndex="0"></TextBox>-->
                                <CustomCombo:AutoCompleteComboBox  x:Name="cmbBloodLoss"  Background="Red" Grid.Row="2" Grid.Column="1"   Margin="4" VerticalAlignment="Center" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID" ></CustomCombo:AutoCompleteComboBox>


                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Dymenorhea" Grid.Row="3" Grid.Column="0" Margin="4" ></TextBlock>
                                <StackPanel Orientation="Horizontal"  Grid.Row="3" Grid.Column="1" VerticalAlignment="Center">
                                    <RadioButton x:Name="rdbDymenorheaY" Content="Yes" Margin="4" ></RadioButton>
                                    <RadioButton x:Name="rdbDymenorheaN" Content="No" Margin="4" IsChecked="True"></RadioButton>
                                </StackPanel>

                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Midcycle Pain" Grid.Row="3" Grid.Column="2" Margin="4" ></TextBlock>
                                <StackPanel Orientation="Horizontal"  Grid.Row="3" Grid.Column="4" VerticalAlignment="Center">
                                    <RadioButton x:Name="rdbMidCyclePainY" Content="Yes" Margin="4" ></RadioButton>
                                    <RadioButton x:Name="rdbMidCyclePainN" Content="No" Margin="4" IsChecked="True"></RadioButton>
                                </StackPanel>

                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Inter Menstrual Bleeding" Grid.Row="4" Grid.Column="0" Margin="4" ></TextBlock>
                                <StackPanel Orientation="Horizontal"  Grid.Row="4" Grid.Column="1" VerticalAlignment="Center">
                                    <RadioButton x:Name="rdbInterMenstrualBleedingY" Content="Yes" Margin="4" ></RadioButton>
                                    <RadioButton x:Name="rdbInterMenstrualBleedingN" Content="No" Margin="4" IsChecked="True"></RadioButton>
                                </StackPanel>

                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Pre Menstrual Tension" Grid.Row="4" Grid.Column="2" Margin="4" ></TextBlock>
                                <StackPanel Orientation="Horizontal"  Grid.Row="4" Grid.Column="4" VerticalAlignment="Center">
                                    <RadioButton x:Name="rdbPreMenstrualTensionY" Content="Yes" Margin="4" ></RadioButton>
                                    <RadioButton x:Name="rdbPreMenstrualTensionN" Content="No" Margin="4" IsChecked="True"></RadioButton>
                                </StackPanel>

                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Dysparuneia" Grid.Row="5" Grid.Column="0" Margin="4" ></TextBlock>
                                <StackPanel Orientation="Horizontal"  Grid.Row="5" Grid.Column="1" VerticalAlignment="Center">
                                    <RadioButton x:Name="rdbDysparuneiaY" Content="Yes" Margin="4" ></RadioButton>
                                    <RadioButton x:Name="rdbDysparuneiaN" Content="No" Margin="4" IsChecked="True"></RadioButton>
                                </StackPanel>

                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Post Coital Bleeding" Grid.Row="5" Grid.Column="2" Margin="4" ></TextBlock>
                                <StackPanel Orientation="Horizontal"  Grid.Row="5" Grid.Column="4" VerticalAlignment="Center">
                                    <RadioButton x:Name="rdbPostCoitalBleedingY" Content="Yes" Margin="4" ></RadioButton>
                                    <RadioButton x:Name="rdbPostCoitalBleedingN" Content="No" Margin="4" IsChecked="True"></RadioButton>
                                </StackPanel>

                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Details Of Past Cycles" Grid.Row="6" Grid.Column="0" Margin="4" ></TextBlock>
                                <TextBox Grid.Row="6" Grid.Column="1" Grid.ColumnSpan="3" Margin="4" Text="{Binding DetailsOfPastCycles, Mode=TwoWay}" x:Name="txtDetailsOfPastCycles"  HorizontalAlignment="Stretch" TabIndex="0"></TextBox>
                            </Grid>
                        </Grp:GroupBox>
                    </controlsToolkit:DockPanel>

                    <controlsToolkit:DockPanel x:Name="docMiddle2" controlsToolkit:DockPanel.Dock="Top" >
                        <Grp:GroupBox x:Name="grpTreatmentHistory" IsTabStop="False" Header="Treatment History" Grid.Row="1" Margin="4" >
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                
                                </Grid.RowDefinitions>

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                    <ColumnDefinition ></ColumnDefinition>
                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                    <ColumnDefinition ></ColumnDefinition>
                                </Grid.ColumnDefinitions>

                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Previous IUI" Grid.Row="0" Grid.Column="0" Margin="4" ></TextBlock>
                                 <StackPanel Orientation="Horizontal"  Grid.Row="0" Grid.Column="1" VerticalAlignment="Center">
                                    <RadioButton x:Name="rdbPreviousIUIY" Content="Yes" Margin="4"></RadioButton>
                                    <RadioButton x:Name="rdbPreviousIUIN" Content="No" Margin="4"  IsChecked="True"></RadioButton>
                                </StackPanel>

                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="No Of Times" Grid.Row="0" Grid.Column="2" Margin="4" ></TextBlock>
