                                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Sperm Concentration (millions/ml)" Margin="4" Grid.Row="0" Grid.ColumnSpan="1"></TextBlock>
                                                                <TextBox  HorizontalAlignment="Stretch" Margin="4" Name="TxtSpermCount" Text="{Binding SpermCount,Mode=TwoWay}" VerticalAlignment="Center"   Grid.Row="0" Grid.Column="1"  TextAlignment="Left" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" LostFocus="txtvolume_LostFocus"  MaxLength="3" />
                                                                <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Left" Text="&gt; 15 mill/ml" Margin="4" Grid.Row="0" Grid.Column="2"></TextBlock>
                                                                <!--<TextBlock Text="(millions/ml)" Grid.Row="0" Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="3" Margin="2,2,80,2"></TextBlock>-->
                                                                <!--<TextBlock Text="> 15 mill/ml - Normal" Grid.Row="0" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="3" ></TextBlock>-->
