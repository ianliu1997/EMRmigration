                                                                    <ColumnDefinition></ColumnDefinition>
                                                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                                    <ColumnDefinition></ColumnDefinition>

                                                                </Grid.ColumnDefinitions>
                                                                <!--<Grp:GroupBox Header="DateTime" x:Name="DateTimeSF" Margin="2" Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" >-->
