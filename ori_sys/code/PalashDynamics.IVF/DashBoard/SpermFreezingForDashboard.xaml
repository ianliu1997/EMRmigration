                                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="5th Percentile" Margin="4" Grid.Row="0" Grid.Column="1"></TextBlock>
                                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" Name="TxtSperm5thPercentile" Text="{Binding Sperm5thPercentile,Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="0" Grid.Column="2"  TextAlignment="Right" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="29" />
                                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="75th Percentile" Margin="4" Grid.Row="0" Grid.Column="3"></TextBlock>
                                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" Name="TxtSperm75thPercentile" Text="{Binding Sperm75thPercentile,Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="0" Grid.Column="4"  TextAlignment="Right" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="29" />
                                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="- Normal" Margin="4" Grid.Row="0" Grid.Column="5"></TextBlock>

