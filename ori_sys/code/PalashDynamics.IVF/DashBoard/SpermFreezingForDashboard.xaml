                                                                    <Grid>
                                                                        <Grid.RowDefinitions>
                                                                            <RowDefinition Height="Auto"></RowDefinition>
                                                                            <RowDefinition Height="Auto"></RowDefinition>
                                                                        </Grid.RowDefinitions>
                                                                        <Grid.ColumnDefinitions>
                                                                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                                            <ColumnDefinition></ColumnDefinition>
                                                                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                                            <ColumnDefinition></ColumnDefinition>
                                                                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                                            <ColumnDefinition></ColumnDefinition>
                                                                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                                            <ColumnDefinition></ColumnDefinition>
                                                                        </Grid.ColumnDefinitions>

                                                                        <TextBlock Text="Freezing Date" Margin="2" Grid.Row="0" Grid.Column="0" VerticalAlignment="Center"></TextBlock>
                                                                        <controls:DatePicker SelectedDate="{Binding Path=ProcDate,Mode=TwoWay}"  x:Name="PrcDate" Margin="2" Grid.Row="0" Grid.Column="1" HorizontalAlignment="Stretch" VerticalAlignment="Center" />

                                                                        <controls:DatePicker x:Name="SpremFreezDate" SelectedDate="{Binding ProcDate, Mode=TwoWay}" Margin="2" Grid.Row="0" TabIndex="1"  Grid.Column="1" HorizontalAlignment="Stretch" VerticalAlignment="Center" LostFocus="SpremFreezDate_LostFocus"/>
                                                                        <TextBlock Text="Freezing Time" Margin="2" Grid.Row="0" Grid.Column="2" VerticalAlignment="Center"></TextBlock>
                                                                        <inputToolkit:TimePicker x:Name="SpremFreezTime" TabIndex="2"   Value="{Binding ProcTime, Mode=TwoWay}" Margin="2" Grid.Row="0"  Grid.Column="3" HorizontalAlignment="Stretch" VerticalAlignment="Center" LostFocus="SpremFreezDate_LostFocus"/>

                                                                        <TextBlock Text="Expiry Date" Grid.Row="0" Grid.Column="4" Margin="4" VerticalAlignment="Center"></TextBlock>
                                                                        <controls:DatePicker  Grid.Row="0" Grid.Column="5"  SelectedDate="{Binding SpremExpiryDate,Mode=TwoWay}"  Margin="4"  x:Name="dtExpiryDate" TabIndex="3"></controls:DatePicker>
                                                                        <TextBlock Text="Expiry Time" Grid.Row="0" Grid.Column="6" Margin="4" VerticalAlignment="Center"></TextBlock>
                                                                        <inputToolkit:TimePicker Margin="4"  Grid.Row="0" Grid.Column="7" Value="{Binding SpremExpiryDate,Mode=TwoWay}"  x:Name="txtExpiryTime" TabIndex="4"></inputToolkit:TimePicker>

                                                                        <TextBlock Text="Abstinence (Days)" HorizontalAlignment="Left" Grid.Row="1" Grid.Column="0" VerticalAlignment="Center" Margin="3"></TextBlock>
                                                                        <!--<TextBox x:Name="txtAbstinence" TabIndex="8"   Text="{Binding OocytePreparationMedia, Mode=TwoWay}" VerticalAlignment="Center" MinWidth="100" Margin="2" Grid.Row="1" Grid.Column="1" ></TextBox>-->
                                                                        <CustomCombo:AutoCompleteComboBox Margin="2,2,45,2"  Grid.Row="1" Grid.Column="1"  x:Name="txtAbstinence"  SelectedValue="{Binding AbstinenceID,Mode=TwoWay}" VerticalAlignment="Center" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID" TabIndex="5"></CustomCombo:AutoCompleteComboBox>
                                                                        <!--<TextBlock Text="(Days)" Grid.Row="1" Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="3" Margin="2,2,45,2"></TextBlock>-->

                                                                        <TextBlock Text="Volume (ml)" HorizontalAlignment="Left" Grid.Row="1" Grid.Column="2" VerticalAlignment="Center" Margin="3"></TextBlock>
                                                                        <TextBox x:Name="txtvolume" TabIndex="6"   Text="{Binding OocytePreparationMedia, Mode=TwoWay}" VerticalAlignment="Center" MinWidth="100" Margin="4" Grid.Row="1" Grid.Column="3" TextChanged="txtvolume_TextChanged" KeyDown="txtvolume_KeyDown" LostFocus="txtvolume_LostFocus" TextAlignment="Right"></TextBox>
                                                                        <!--<TextBlock Text="(ml)" Grid.Row="1" Grid.Column="3" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="3" Margin="2,2,30,2"></TextBlock>-->
                                                                        <TextBlock Text="&lt; 1.5-Low | 2.0- Normal | &gt; 2.0-High" Grid.Row="1" Grid.Column="4" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="3" Grid.ColumnSpan="2" ></TextBlock>

                                                                        <TextBlock Text="Spillage" HorizontalAlignment="Left" Grid.Row="1" Grid.Column="6" VerticalAlignment="Center" Margin="3"></TextBlock>
                                                                        <TextBox x:Name="txtSpillage" TabIndex="7"   Text="{Binding spillage, Mode=TwoWay}" VerticalAlignment="Center" MinWidth="100" Margin="4" Grid.Row="1" Grid.Column="7" MaxLength="30" ></TextBox>

                                                                    </Grid>
                                                                </Grp:GroupBox>

                                                                <!--Change-->
                                                                <!--<Grp:GroupBox Header="Doctor" x:Name="DoctorSF" Margin="2" Grid.Row="1" Grid.Column="2" Grid.ColumnSpan="2" Visibility="Collapsed">
                                                                    <Grid>
                                                                        <Grid.RowDefinitions>
                                                                            <RowDefinition Height="Auto"></RowDefinition>
                                                                            <RowDefinition Height="Auto"></RowDefinition>
                                                                        </Grid.RowDefinitions>
                                                                        <Grid.ColumnDefinitions>
                                                                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                                            <ColumnDefinition></ColumnDefinition>
                                                                        </Grid.ColumnDefinitions>
                                                                        <TextBlock Grid.Row="0" Grid.Column="0" Text="Doctor "  VerticalAlignment="Center" Margin="3"></TextBlock>
                                                                        <inputcombo:AutoCompleteComboBox  x:Name="cmbDoctorName" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID" VerticalAlignment="Center" MinWidth="100"  Margin="2" TabIndex="3"    Grid.Row="0" Grid.Column="1" ></inputcombo:AutoCompleteComboBox>

                                                                        <TextBlock Grid.Row="1" Grid.Column="0" Text="Embryologist" Margin="3" VerticalAlignment="Center"></TextBlock>
                                                                        <inputcombo:AutoCompleteComboBox  x:Name="cmbEmbryologist" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID" VerticalAlignment="Center" MinWidth="100" Margin="2"  Grid.Row="1" Grid.Column="1 "  TabIndex="4"></inputcombo:AutoCompleteComboBox>
                                                                    </Grid>
                                                                </Grp:GroupBox>-->

                                                            </Grid>

                                                        </Grid>
                                                    </StackPanel>
                                                    <!-- Semen Details-->

                                                    <!--<StackPanel Grid.Row="2" Margin="1">-->
                                                    <!--<Grid Grid.Row="2" Margin="1">
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="Auto"></RowDefinition>
                                                                <RowDefinition Height="Auto"></RowDefinition>
                                                            </Grid.RowDefinitions>
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="1*"></ColumnDefinition>
                                                                <ColumnDefinition Width="1*"></ColumnDefinition>

                                                            </Grid.ColumnDefinitions>-->

