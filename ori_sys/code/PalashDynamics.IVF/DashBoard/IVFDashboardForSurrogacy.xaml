                                                                                    </Grid.RowDefinitions>

                                                                                    <TextBlock Grid.Column="0" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Text="Multiple Pregnancy" Margin="4" ></TextBlock>
                                                                                    <CustomCombo:AutoCompleteComboBox  Grid.Column="1" Grid.Row="0"  Width="130" x:Name="cmbBabyType" HorizontalAlignment="Left"  SelectedItemBinding="{Binding MasterListItem, Mode=TwoWay}" SelectedValuePath="ID"  VerticalAlignment="Center" Text="" Margin="4"/>

                                                                                </Grid>
                                                                            </StackPanel>
                                                                        </Grid>

                                                                    </Grp:GroupBox>

                                                                    <Grp:GroupBox Grid.Row="6" Header=" OHSS Complications (Female)  " Margin=" 3">
                                                                        <Grid>
                                                                            <Grid.ColumnDefinitions>
                                                                                <ColumnDefinition Width="Auto"/>
                                                                                <ColumnDefinition Width="Auto"/>
                                                                                <ColumnDefinition />
                                                                            </Grid.ColumnDefinitions>
                                                                            <Grid.RowDefinitions>
                                                                                <RowDefinition Height="Auto"/>
                                                                                <RowDefinition Height="Auto"/>
                                                                                <RowDefinition Height="Auto"/>
                                                                            </Grid.RowDefinitions>

                                                                            <CheckBox x:Name="chkOHSSEarly" Content="Early" Grid.Column="0" Grid.Row="0" Margin="4" IsChecked="{Binding OHSSEarly,Mode=TwoWay}"></CheckBox>
                                                                            <CheckBox x:Name="chkOHSSLate" Content="Late"  Grid.Column="1" Grid.Row="0" Margin="4" IsChecked="{Binding OHSSLate,Mode=TwoWay}" ></CheckBox>

                                                                            <CheckBox x:Name="chkOHSSmild" Content="Mild" Grid.Column="0" Grid.Row="1"  Margin="4" IsChecked="{Binding OHSSMild,Mode=TwoWay}"></CheckBox>
                                                                            <CheckBox x:Name="chkOHSSmod" Content="Mode"  Grid.Column="1" Grid.Row="1" Margin="4" IsChecked="{Binding OHSSMode,Mode=TwoWay}" ></CheckBox>
                                                                            <CheckBox x:Name="chkOHSSsevere" Content="severe" Grid.Column="2" Grid.Row="1" Margin="4"  IsChecked="{Binding OHSSSereve,Mode=TwoWay}"></CheckBox>

                                                                            <TextBlock Grid.Column="0" Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Right" Text="Remark" Margin="4" Height="30"></TextBlock>
                                                                            <TextBox x:Name="txtOHSSremark" Text="{Binding OHSSRemark,Mode=TwoWay}" Grid.ColumnSpan="2" Grid.Column="1" Grid.Row="2" Margin="4" AcceptsReturn="True" Height="50" VerticalScrollBarVisibility="Visible"  ></TextBox>

                                                                        </Grid>
                                                                    </Grp:GroupBox>
                                                                </Grid>

                                                                <StackPanel  Orientation="Horizontal"  Margin="0,0,0,0" HorizontalAlignment="Right" Grid.Row="1" Grid.Column="1">
                                                                    <toolkit:DockPanel toolkit:DockPanel.Dock="Bottom" HorizontalAlignment="Right" VerticalAlignment="Center" >
                                                                        <Border BorderBrush="{StaticResource TabControlDarkerBorder}"  Margin="3" controlsToolkit:DockPanel.Dock="Bottom" BorderThickness="1" CornerRadius="3,3,3,3" Background="Transparent">
                                                                            <StackPanel  Orientation="Horizontal" VerticalAlignment="Center"   Margin="3" HorizontalAlignment="Right" Background="Transparent">
                                                                                <StackPanel  Orientation="Horizontal"  Margin="0,0,0,0">
                                                                                    <ToggleButton x:Name="cmdSaveOut"  Background="#000000" Margin="5,0,0,0" HorizontalAlignment="Stretch" Style="{StaticResource ToggleButtonStyle}" Visibility="Visible" Click="cmdSave_Click" TabIndex="3">
                                                                                        <StackPanel Orientation="Horizontal" >
                                                                                            <Common:XamlCheckmark Width="10" Height="10" Margin="0,0,5,0"/>
                                                                                            <TextBlock Text="Save" TextWrapping="Wrap"/>
                                                                                        </StackPanel>
                                                                                    </ToggleButton>
                                                                                </StackPanel>
                                                                            </StackPanel>
                                                                        </Border>
                                                                    </toolkit:DockPanel>
                                                                </StackPanel>
                                                            </Grid>
                                                        </controlsToolkit:DockPanel>
                                                    </toolkit:DockPanel>
                                                </ScrollViewer>
                                            </toolkit:DockPanel>
                                        </sdk:TabItem>

                                        <!-- BirthDeatails TAB-->
                                        <sdk:TabItem  x:Name="Tabbirthdetails" Visibility="Collapsed" Style="{StaticResource TabItemStyle}"  Padding="0" Margin="0" Header="Birth Details" >
                                            <controlsToolkit:DockPanel LastChildFill="True" Visibility="Visible">
                                                <toolkit:DockPanel toolkit:DockPanel.Dock="Top" Visibility="Visible">
                                                    <toolkit:DockPanel x:Name="docTabbirthdetails" toolkit:DockPanel.Dock="Top" HorizontalAlignment="Stretch">
                                                        <Border  BorderBrush="{StaticResource TabControlDarkerBorder}" toolkit:DockPanel.Dock="Top" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0,-1,0,0" BorderThickness="1" CornerRadius="0,0,3,3" Background="Transparent">
                                                            <ScrollViewer VerticalScrollBarVisibility="Visible" VerticalAlignment="Stretch">
                                                                <Grid ScrollViewer.VerticalScrollBarVisibility="Auto" VerticalAlignment="Stretch">
                                                                    <Grid.RowDefinitions>
                                                                        <RowDefinition></RowDefinition>
                                                                    </Grid.RowDefinitions>
                                                                    <ContentControl Template="{StaticResource FilledContentControlTemplate}"  Background="Aqua"  x:Name="BirthContent" ScrollViewer.VerticalScrollBarVisibility="Visible" VerticalAlignment="Stretch"   ScrollViewer.HorizontalScrollBarVisibility="Auto" />
                                                                </Grid>
                                                            </ScrollViewer>
                                                        </Border>
                                                    </toolkit:DockPanel>
                                                </toolkit:DockPanel>
                                            </controlsToolkit:DockPanel>
                                        </sdk:TabItem>

                                    </sdk:TabControl>
                                </Border>
                            </controlsToolkit:DockPanel>

                        </controlsToolkit:DockPanel>
                    </controlsToolkit:DockPanel>
                </Grid>
            </Grid>

        </controlsToolkit:DockPanel>
    </Grid>
</UserControl>