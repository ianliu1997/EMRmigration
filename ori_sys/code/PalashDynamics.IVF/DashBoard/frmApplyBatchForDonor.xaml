<controls:ChildWindow x:Class="PalashDynamics.IVF.DashBoard.frmApplyBatchForDonor"
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      xmlns:data="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Data"
           xmlns:controls="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls"
                         xmlns:CustomCombo="clr-namespace:PalashDynamics.Controls;assembly=PalashDynamics.Controls.AutoCompleteComboBox"
                        xmlns:Grp="clr-namespace:Groupbox;assembly=Groupbox" 
                       xmlns:Converter="clr-namespace:PalashDynamics.Converters;assembly=PalashDynamics.Converters"
                       xmlns:inputcombo="clr-namespace:PalashDynamics.Controls;assembly=PalashDynamics.Controls.AutoCompleteComboBox"
                        xmlns:input="clr-namespace:PalashDynamics.Controls;assembly=PalashDynamics.Controls.AutoCompleteComboBox"
            xmlns:controlsToolkit="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Toolkit" 
           Width="800" Height="600" 
           Title="frmApplyBatchForDonor" Loaded="ChildWindow_Loaded">
    <controls:ChildWindow.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/PalashDynamics;component/Generic.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <Converter:DateTimeToAgePart x:Key="DateTimeToAgePart"></Converter:DateTimeToAgePart>
            <Converter:DateTimeConverter x:Key="DateConverter"></Converter:DateTimeConverter>
            <ControlTemplate x:Key="FilledContentControlTemplate" TargetType="ContentControl">
                <Grid>
                    <ContentPresenter />
                </Grid>
            </ControlTemplate>
        </ResourceDictionary>
    </controls:ChildWindow.Resources>
    <Grid x:Name="LayoutRoot" Background="White" Margin="2">

        <controlsToolkit:DockPanel LastChildFill="True" Margin="4">
            <Border BorderBrush="{StaticResource TabControlDarkerBorder}"  Margin="4" controlsToolkit:DockPanel.Dock="Bottom" BorderThickness="1" CornerRadius="3,3,3,3" Background="Transparent">
                <StackPanel x:Name="FooterBackPanel" Orientation="Horizontal"    Margin="2" HorizontalAlignment="Right" Background="Transparent">
                    <StackPanel x:Name="SaveCommandPanel" Orientation="Horizontal"  Margin="0,0,0,0" Height="25" VerticalAlignment="Bottom" HorizontalAlignment="Center">

                        <ToggleButton x:Name="cmdSave" Content="Save" Background="#000000" Margin="5,0,0,0" HorizontalAlignment="Stretch" Style="{StaticResource ToggleButtonStyle}" Visibility="Visible" Click="cmdSave_Click" TabIndex="23">

                        </ToggleButton>
                        <ToggleButton x:Name="CmdClose"  Grid.Row="4" Content="Close" MinWidth="50" Background="#000000" Margin="5,0,0,0" HorizontalAlignment="Stretch" Style="{StaticResource ToggleButtonStyle}" Visibility="Visible" Click="CmdClose_Click" TabIndex="24" >

                        </ToggleButton>
                    </StackPanel>
                </StackPanel>
            </Border>

            <Border BorderBrush="{StaticResource TabControlDarkerBorder}"  Margin="4" BorderThickness="1" CornerRadius="3,3,3,3" Background="Transparent">
                <ScrollViewer VerticalScrollBarVisibility="Visible">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Grp:GroupBox Header="New Batch" x:Name="NewBatch" Grid.Row="0">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                    <ColumnDefinition ></ColumnDefinition>
                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                    <ColumnDefinition ></ColumnDefinition>
                                    <ColumnDefinition Width="Auto"></ColumnDefinition>



                                </Grid.ColumnDefinitions>

                                <TextBlock Text="Lab" Grid.Row="0" Grid.Column="0" Margin="2,2,2,2" VerticalAlignment="Center" HorizontalAlignment="Right" ></TextBlock>
                                <CustomCombo:AutoCompleteComboBox x:Name="cmbLab"  Grid.Row="0" Grid.Column="1"   Margin="2" VerticalAlignment="Center"   SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID" ></CustomCombo:AutoCompleteComboBox>
                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Left" Text="Invoice Number" Grid.Row="0" Grid.Column="2" Margin="2,2,2,2" ></TextBlock>
                                <TextBox Grid.Row="0" Grid.Column="3" Margin="4" Text="{Binding Invoice Number, Mode=TwoWay}" x:Name="txtInvoiceNumber" MaxLength="50" ></TextBox>
                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Received Date" Grid.Row="1" Grid.Column="0" Margin="2,2,2,2" ></TextBlock>
                                <controls:DatePicker Grid.Row="1" Grid.Column="1" Margin="4"   x:Name="dtpReceivedDate"  SelectedDate="{Binding ReceivedDate, Mode=TwoWay}" ></controls:DatePicker>
                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Left" Text="Received By" Grid.Row="1" Grid.Column="2" Margin="2,2,2,2" ></TextBlock>
                                <CustomCombo:AutoCompleteComboBox x:Name="cmbReceivedBy"  Grid.Row="1" Grid.Column="3"   Margin="2" VerticalAlignment="Center"   SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID" ></CustomCombo:AutoCompleteComboBox>
                                <TextBlock Text="Batch" Grid.Row="2" Grid.Column="0" Margin="2,2,2,2" VerticalAlignment="Center" HorizontalAlignment="Right" ></TextBlock>
                                <TextBox Grid.Row="2" Grid.Column="1" Margin="4" Text="{Binding Invoice Number, Mode=TwoWay}" x:Name="txtBatch"  ></TextBox>
                                <TextBlock Text="No. of Vials" Grid.Row="2" Grid.Column="2" Margin="2,2,2,2" VerticalAlignment="Center" HorizontalAlignment="Left" ></TextBlock>
                                <TextBox Grid.Row="2" Grid.Column="3" Margin="4" Text="{Binding Invoice Number, Mode=TwoWay}" x:Name="txtNoofVials"  TextChanged="txtNoofVials_TextChanged" KeyDown="txtNoofVials_KeyDown" MaxLength="1" ></TextBox>
                                <TextBlock Text="Remark" Grid.Row="3" Grid.Column="0" Margin="2,2,2,2" VerticalAlignment="Center" HorizontalAlignment="Right" ></TextBlock>
                                <TextBox Grid.Row="3" Grid.Column="1"  Grid.ColumnSpan="3" Margin="4" Text="{Binding Remark, Mode=TwoWay}" x:Name="txtRemark" VerticalScrollBarVisibility="Visible" TextWrapping="Wrap" ></TextBox>
                            </Grid>
                        </Grp:GroupBox>
                        <Grp:GroupBox Header="Semen Freezing Details" x:Name="SemenFreezing" Grid.Row="1">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>

                                <Grp:GroupBox Header="Pre-FreezingDetails" x:Name="FreezingDetailsSF" Grid.Column="0" Grid.ColumnSpan="2">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition />
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition />

                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>

                                        </Grid.RowDefinitions>

                                        <TextBlock Text="Volume" HorizontalAlignment="Left" Grid.Row="0" Grid.Column="0" VerticalAlignment="Center" Margin="3"></TextBlock>
                                        <TextBox x:Name="txtvolume"   Text="{Binding OocytePreparationMedia, Mode=TwoWay}" VerticalAlignment="Center"  Margin="4" Grid.Row="0" Grid.Column="1" TextChanged="txtvolume_TextChanged" KeyDown="txtvolume_KeyDown" ></TextBox>
                                        <TextBlock Text="(ml)" Grid.Row="0" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="3"></TextBlock>

                                        <TextBlock Text="Abstinence" HorizontalAlignment="Left" Grid.Row="0" Grid.Column="3" VerticalAlignment="Center" Margin="4"></TextBlock>
                                        <TextBox x:Name="txtAbstinence"   Text="{Binding OocytePreparationMedia, Mode=TwoWay}" VerticalAlignment="Center"  Margin="4" Grid.Row="0" Grid.Column="4" ></TextBox>

                                        <TextBlock Text="Total Sperm Count" HorizontalAlignment="Left" Grid.Row="1" Grid.Column="0" VerticalAlignment="Center" Margin="3"></TextBlock>
                                        <TextBox x:Name="txtTotalSpremCount"    Text="{Binding OocytePreparationMedia, Mode=TwoWay}" VerticalAlignment="Center" Margin="4"  Grid.Row="1" Grid.Column="1" TextChanged="txtTotalSpremCount_TextChanged" KeyDown="txtTotalSpremCount_KeyDown"></TextBox>
                                        <TextBlock Text="(Milllion)" Grid.Row="1" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="4"></TextBlock>

                                        <TextBlock Text="Motility" HorizontalAlignment="Left" Grid.Row="2" Grid.Column="0" VerticalAlignment="Center" Margin="4"></TextBlock>
                                        <TextBox x:Name="txtMotility"    Text="{Binding OocytePreparationMedia, Mode=TwoWay}" VerticalAlignment="Center"  Margin="4"  Grid.Row="2" Grid.Column="1" TextChanged="txtMotility_TextChanged" KeyDown="txtMotility_KeyDown"  ></TextBox>
                                        <TextBlock Text="(%)" Grid.Row="2" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="4"></TextBlock>

                                        <TextBlock Text="Viscosity" HorizontalAlignment="Left" Grid.Row="1" Grid.Column="3" VerticalAlignment="Center" Margin="3"></TextBlock>
                                        <inputcombo:AutoCompleteComboBox   x:Name="cmbViscosity" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID" VerticalAlignment="Center" Margin="2"  Grid.Row="1" Grid.Column="4"  ></inputcombo:AutoCompleteComboBox>


                                    </Grid>
                                </Grp:GroupBox>

                                <Grp:GroupBox Header=" Grade" Margin="2" Grid.Row="1" Grid.Column="0"  x:Name="GradeSF">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"></RowDefinition>
                                            <RowDefinition Height="Auto"></RowDefinition>
                                            <RowDefinition Height="Auto"></RowDefinition>
                                            <RowDefinition Height="Auto"></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                                            <ColumnDefinition Width="1*"></ColumnDefinition>
                                            <ColumnDefinition Width="1*"></ColumnDefinition>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock Text="Grade A " HorizontalAlignment="Left" Grid.Row="0" Grid.Column="0" VerticalAlignment="Center"  Margin="3"></TextBlock>
                                        <TextBox x:Name="txtGradeA" TabIndex="12"   Text="{Binding OocytePreparationMedia, Mode=TwoWay}" VerticalAlignment="Center" MinWidth="100" Margin="2" Grid.Row="0" Grid.Column="1" KeyDown="txtGradeA_KeyDown" TextChanged="txtGradeA_TextChanged"></TextBox>
                                        <TextBlock Text="( % )" HorizontalAlignment="Left" Grid.Row="0" Grid.Column="3" VerticalAlignment="Center" Margin="3"></TextBlock>

                                        <TextBlock Text="Grade B " HorizontalAlignment="Left" Grid.Row="1" Grid.Column="0" VerticalAlignment="Center"  Margin="3"></TextBlock>
                                        <TextBox x:Name="txtGradeB" TabIndex="13"   Text="{Binding OocytePreparationMedia, Mode=TwoWay}" VerticalAlignment="Center" MinWidth="100" Margin="2" Grid.Row="1" Grid.Column="1" KeyDown="txtGradeB_KeyDown" TextChanged="txtGradeB_TextChanged" ></TextBox>
                                        <TextBlock Text="( % )" HorizontalAlignment="Left" Grid.Row="1" Grid.Column="3" VerticalAlignment="Center" Margin="3"></TextBlock>

                                        <TextBlock Text="Grade C " HorizontalAlignment="Left" Grid.Row="2" Grid.Column="0" VerticalAlignment="Center"  Margin="3"></TextBlock>
                                        <TextBox x:Name="txtGradeC" TabIndex="14"   Text="{Binding OocytePreparationMedia, Mode=TwoWay}" VerticalAlignment="Center" MinWidth="100" Margin="2" Grid.Row="2" Grid.Column="1" KeyDown="txtGradeC_KeyDown" TextChanged="txtGradeC_TextChanged" ></TextBox>
                                        <TextBlock Text="( % )" HorizontalAlignment="Left" Grid.Row="2" Grid.Column="3" VerticalAlignment="Center" Margin="3"></TextBlock>
                                    </Grid>
                                </Grp:GroupBox>

                                <Grp:GroupBox Header=" Freezing Details" Margin="2" Grid.Row="1" Grid.Column="1" x:Name="DetailsSF">
                                    <Grid HorizontalAlignment="Stretch"  Grid.RowSpan="2">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"></RowDefinition>
                                            <RowDefinition Height="Auto"></RowDefinition>

                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                                            <ColumnDefinition ></ColumnDefinition>

                                        </Grid.ColumnDefinitions>

                                        <TextBlock Text=" Other Details " HorizontalAlignment="Right" Grid.Row="0" Grid.Column="0" VerticalAlignment="Center"  Margin="3"></TextBlock>
                                        <TextBox x:Name="txtOtherDetails" TabIndex="15"   Text="{Binding OocytePreparationMedia, Mode=TwoWay}" Grid.ColumnSpan="2" VerticalAlignment="Center" MinWidth="100" Margin="4" Grid.Row="0" Grid.Column="1" ></TextBox>

                                        <TextBlock Text="Comments" HorizontalAlignment="Right" Grid.Row="1"  Grid.Column="0" VerticalAlignment="Center"  Margin="3"></TextBlock>
                                        <TextBox x:Name="txtComments" TabIndex="16"  Text="{Binding OocytePreparationMedia, Mode=TwoWay}" VerticalAlignment="Center" MinWidth="100" Grid.ColumnSpan="2" VerticalScrollBarVisibility="Visible"  TextWrapping="Wrap" AcceptsReturn="True" Margin="4"  Grid.RowSpan="3" Grid.Row="1" Grid.Column="1" ></TextBox>

                                    </Grid>
                                </Grp:GroupBox>










                            </Grid>
                        </Grp:GroupBox>

                        <Border controlsToolkit:DockPanel.Dock="Top"   BorderBrush="{StaticResource TabControlDarkerBorder}"  Grid.Row="2"  BorderThickness="1" Margin="3" CornerRadius="3,3,0,0"  Background="Transparent" >
                            <ContentControl VerticalAlignment="Center" FontSize="12" Content="Sperm Freezing Details " Margin="-1" Style="{StaticResource Header}"     HorizontalAlignment="Stretch" />
                        </Border>


                        <data:DataGrid  SelectionMode="Single" Grid.Row="3" x:Name="dgSpremFreezingDetailsGrid" ItemsSource="{Binding}"  AutoGenerateColumns="False" IsReadOnly="True" VerticalAlignment="Stretch" Height="160" LoadingRow="dgSpremFreezingDetailsGrid_LoadingRow" >
                            <data:DataGrid.Columns>
                                <data:DataGridTextColumn Width="100" Binding="{Binding SpremNostr}" Header="Collection No."/>   
                                <data:DataGridTemplateColumn Header="Tank">
                                    <data:DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <Grid>
                                                <input:AutoCompleteComboBox VerticalAlignment="Center" x:Name="cmbTank" ItemsSource="{Binding TankListVO}"   SelectedItem="{Binding selectedTankListVO,Mode=TwoWay}"  SelectedValue="{Binding selectedTankListVO.ID}"  Text="" SelectionChanged="cmbGrade_SelectionChanged"/>
                                            </Grid>
                                        </DataTemplate>
                                    </data:DataGridTemplateColumn.CellTemplate>
                                </data:DataGridTemplateColumn>


                                <data:DataGridTemplateColumn Header="Canister">
                                    <data:DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <Grid>
                                                <input:AutoCompleteComboBox VerticalAlignment="Center" x:Name="cmbCanister" ItemsSource="{Binding CanisterListVO}"   SelectedItem="{Binding selectedCanisterListVO,Mode=TwoWay}"  SelectedValue="{Binding selectedCanisterListVO.ID}"  Text="" SelectionChanged="cmbGrade_SelectionChanged"/>
                                            </Grid>
                                        </DataTemplate>
                                    </data:DataGridTemplateColumn.CellTemplate>
                                </data:DataGridTemplateColumn>

                                <data:DataGridTemplateColumn Header="Cane">
                                    <data:DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <Grid>
                                                <input:AutoCompleteComboBox VerticalAlignment="Center" x:Name="cmbCane" ItemsSource="{Binding CanListVO}"   SelectedItem="{Binding selectedCanListVO,Mode=TwoWay}"  SelectedValue="{Binding selectedCanListVO.ID}"  Text="" SelectionChanged="cmbGrade_SelectionChanged"/>
                                            </Grid>
                                        </DataTemplate>
                                    </data:DataGridTemplateColumn.CellTemplate>
                                </data:DataGridTemplateColumn>
                                <data:DataGridTemplateColumn Header="Goblet Code">
                                    <data:DataGridTemplateColumn.CellTemplate >
                                        <DataTemplate>
                                            <Grid >
                                                <input:AutoCompleteComboBox TabIndex="17"   VerticalAlignment="Center" x:Name="cmbcolor" ItemsSource="{Binding ColorListVO}"   SelectedItem="{Binding selectedColorList,Mode=TwoWay}" SelectedValue="{Binding selectedColorList.ID}"  Text="" SelectionChanged="cmbGrade_SelectionChanged"/>
                                            </Grid>
                                        </DataTemplate>
                                    </data:DataGridTemplateColumn.CellTemplate>
                                </data:DataGridTemplateColumn>

                                <data:DataGridTemplateColumn Header="Straw">
                                    <data:DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <Grid>
                                                <input:AutoCompleteComboBox VerticalAlignment="Center" x:Name="cmbStraw" ItemsSource="{Binding StrawListVO}"   SelectedItem="{Binding selectedStrawListVO,Mode=TwoWay}"  SelectedValue="{Binding selectedStrawListVO.ID}"  Text="" SelectionChanged="cmbGrade_SelectionChanged"/>
                                            </Grid>
                                        </DataTemplate>
                                    </data:DataGridTemplateColumn.CellTemplate>
                                </data:DataGridTemplateColumn>

                                <data:DataGridTemplateColumn Header="Goblet Shape" Visibility="Collapsed">
                                    <data:DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <Grid>
                                                <input:AutoCompleteComboBox VerticalAlignment="Center" x:Name="cmbGlobletShape" ItemsSource="{Binding GlobletShapeListVO}"   SelectedItem="{Binding selectedGlobletShapeListVO,Mode=TwoWay}"  SelectedValue="{Binding selectedGlobletShapeListVO.ID}"  Text="" SelectionChanged="cmbGrade_SelectionChanged"/>
                                            </Grid>
                                        </DataTemplate>
                                    </data:DataGridTemplateColumn.CellTemplate>
                                </data:DataGridTemplateColumn>

                                <data:DataGridTemplateColumn Header="Goblet Size" Visibility="Collapsed">
                                    <data:DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <Grid>
                                                <input:AutoCompleteComboBox VerticalAlignment="Center" x:Name="cmbGlobletSize" ItemsSource="{Binding GlobletSizeListVO}"   SelectedItem="{Binding selectedGlobletSizeListVO,Mode=TwoWay}"  SelectedValue="{Binding selectedGlobletSizeListVO.ID}"  Text="" SelectionChanged="cmbGrade_SelectionChanged"/>
                                            </Grid>
                                        </DataTemplate>
                                    </data:DataGridTemplateColumn.CellTemplate>
                                </data:DataGridTemplateColumn>

                                <data:DataGridTemplateColumn Header="Comments">
                                    <data:DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <Grid>
                                                <TextBox Width="100" TextWrapping="Wrap" ScrollViewer.VerticalScrollBarVisibility="Visible" AcceptsReturn="True" VerticalAlignment="Center"  Height="28"  Text="{Binding Comments,Mode=TwoWay}" ></TextBox>
                                            </Grid>
                                        </DataTemplate>
                                    </data:DataGridTemplateColumn.CellTemplate>
                                </data:DataGridTemplateColumn>

                                <data:DataGridTemplateColumn>
                                    <data:DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <HyperlinkButton VerticalAlignment="Center" Padding="5,0,5,0" Margin="4"  IsTabStop="False" Click="btnAdd_Click" x:Name="btnAdd" HorizontalAlignment="Center" Content="Add" IsEnabled="{Binding IsAdd}" Visibility="Collapsed"></HyperlinkButton>
                                        </DataTemplate>
                                    </data:DataGridTemplateColumn.CellTemplate>
                                </data:DataGridTemplateColumn>

                                <data:DataGridTemplateColumn>
                                    <data:DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <HyperlinkButton VerticalAlignment="Center" FontFamily="Webdings" Content="r"  Padding="5,0,5,0" IsTabStop="False" Click ="cmdDelete_Click"  x:Name="cmdDelete" ToolTipService.ToolTip="Delete" IsEnabled="{Binding IsFreezReadOnly}" Visibility="Collapsed"></HyperlinkButton>
                                        </DataTemplate>
                                    </data:DataGridTemplateColumn.CellTemplate>
                                </data:DataGridTemplateColumn>
                            </data:DataGrid.Columns>
                        </data:DataGrid>

                    </Grid>
                </ScrollViewer>
            </Border>


        </controlsToolkit:DockPanel>



    </Grid>
</controls:ChildWindow>
