<controls:ChildWindow x:Class="PalashDynamics.IVF.DashBoard.SemenExamination_Dashboard"
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
           xmlns:inputToolkit="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Input.Toolkit"
           xmlns:input="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Input"
           xmlns:controlsToolkit="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Toolkit" 
           xmlns:controls="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls"
           xmlns:Grp="clr-namespace:Groupbox;assembly=Groupbox" 
           xmlns:CustomCombo="clr-namespace:PalashDynamics.Controls;assembly=PalashDynamics.Controls.AutoCompleteComboBox"
           xmlns:local="clr-namespace:PalashDynamics.Converters;assembly=PalashDynamics.Converters" 
           xmlns:data="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"       
           Width="1450" Height="700" 
           Title="Semen Examination"
           Loaded="SemenExamination_Dashboard_Loaded" >

    <controls:ChildWindow.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/PalashDynamics;component/Generic.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <local:DateTimeConverter x:Key="DateConverter" />
        </ResourceDictionary>
    </controls:ChildWindow.Resources>

    <Grid x:Name="Root" Background="White" ScrollViewer.VerticalScrollBarVisibility="Visible">
        <Grid x:Name="DisplayArea">
            <Grid.RowDefinitions>
                <RowDefinition/>

            </Grid.RowDefinitions>
            <controlsToolkit:DockPanel LastChildFill="True" Margin="4">
                <Border BorderBrush="{StaticResource TabControlDarkerBorder}"  Margin="4" controlsToolkit:DockPanel.Dock="Bottom" BorderThickness="1" CornerRadius="3,3,3,3" Background="Transparent">
                    <StackPanel  x:Name="Footer" Orientation="Horizontal" VerticalAlignment="Center" Margin="0,3,5,3" HorizontalAlignment="Right" Background="Transparent">
                        <StackPanel x:Name="SavePAnel" Orientation="Horizontal"  Margin="0,0,0,0">
                            <ToggleButton x:Name="cmdNew" Content="New"  Background="#000000" Margin="5,0,0,0" HorizontalAlignment="Stretch" Style="{StaticResource ToggleButtonStyle}" Visibility="Visible" Click="cmdNew_Click" ></ToggleButton>
                            <ToggleButton  x:Name="CmdSave"  Grid.Row="4" Content="Save" MinWidth="50"  Background="#000000" Margin="5,0,0,0" HorizontalAlignment="Stretch"  Style="{StaticResource ToggleButtonStyle}" Click="CmdSave_Click" TabIndex="8"/>
                            <ToggleButton x:Name="CmdClose"  Grid.Row="4" Content="Cancel" MinWidth="50" Background="#000000" Margin="5,0,0,0" HorizontalAlignment="Stretch" Style="{StaticResource ToggleButtonStyle}" Visibility="Visible"  TabIndex="9"  Click="CancelButton_Click"/>
                            <ToggleButton x:Name="CmdPrint"  Grid.Row="4" Content="Print" MinWidth="50" Background="#000000" Margin="5,0,0,0" HorizontalAlignment="Stretch" Style="{StaticResource ToggleButtonStyle}" Visibility="Collapsed" TabIndex="19" />
                            <!--Click="CmdPrint_Click"-->
                        </StackPanel>
                    </StackPanel>
                </Border>


                <Border BorderBrush="{StaticResource TabControlDarkerBorder}"   BorderThickness="1" CornerRadius="3,3,3,3" Background="Transparent">
                    <Grid x:Name="PageContainer" >


                        <Grid Height="AUTO" Visibility="Visible" Width="auto" x:Name="FrontPanel" Grid.Row="0">
                            <controlsToolkit:DockPanel x:Name="docList">


                                <StackPanel controlsToolkit:DockPanel.Dock="Top">
                                    <Border  BorderBrush="{StaticResource TabControlDarkerBorder}"   BorderThickness="1" CornerRadius="3,3,0,0"  Background="Transparent">
                                        <ContentControl VerticalAlignment="Center" FontSize="12" Content="List Of Semen Examination Details" Margin="-1" Style="{StaticResource Header}" Name="first"  HorizontalAlignment="Stretch" />
                                    </Border>
                                </StackPanel>

                                <data:DataPager x:Name="dgDataPager" controlsToolkit:DockPanel.Dock="Bottom" 
                                                        DisplayMode="FirstLastPreviousNext"  IsEnabled="True"                       
                                                        Source="{Binding}" 
                                                        AutoEllipsis="False" IsTotalItemCountFixed="False"  />

                                <data:DataGrid x:Name="dgSemenExamination" AutoGenerateColumns="False" ItemsSource="{Binding}" VerticalScrollBarVisibility="Visible" IsReadOnly="True" Margin="2" HorizontalAlignment="Stretch" CanUserSortColumns="False">
                                    <data:DataGrid.Columns>
                                        <data:DataGridTemplateColumn>
                                            <data:DataGridTemplateColumn.CellTemplate>

                                                <DataTemplate>
                                                    <HyperlinkButton VerticalAlignment="Center"  Content="View"  Padding="5,0,5,0"   x:Name="hlbViewExamination" Click="hlbViewExamination_Click" ></HyperlinkButton>
                                                </DataTemplate>
                                            </data:DataGridTemplateColumn.CellTemplate>
                                        </data:DataGridTemplateColumn>
                                        <data:DataGridTextColumn Binding="{Binding CollectionDate,Converter={StaticResource DateConverter},ConverterParameter={StaticResource GridDateDisplayFormat}}" Header="Date"/>
                                        <data:DataGridTextColumn Binding="{Binding MethodOfCollection}" Header="Method Of Collection" />

                                        <data:DataGridTemplateColumn >
                                            <data:DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <HyperlinkButton ToolTipService.ToolTip="Print Details" VerticalAlignment="Center" Padding="2" IsTabStop="False" Click="HyperlinkButton_Click">
                                                        <Image  Height="16" Stretch="Fill" Source="/PalashDynamics;component/Icons/Print_32.png"/>
                                                    </HyperlinkButton>
                                                </DataTemplate>
                                            </data:DataGridTemplateColumn.CellTemplate>
                                        </data:DataGridTemplateColumn>
                                    </data:DataGrid.Columns>
                                </data:DataGrid>

                            </controlsToolkit:DockPanel>


                        </Grid>

                        <Grid  Height="AUTO" Width="auto" x:Name="BackPanel" Visibility="Collapsed" Grid.Row="0">
                            <controlsToolkit:DockPanel LastChildFill="True" Margin="4">
                                <ScrollViewer VerticalScrollBarVisibility="Visible">
                                    <Grid Width="Auto"  Margin="10"  DataContext="{Binding BizActionObject}">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <Grp:GroupBox IsTabStop="False" x:Name="grpContactDetails" Header="Preliminary Detail" Height="Auto" Grid.Row="1" Grid.Column="0" Margin="4" >
                                            <Grid  Background="White"  Visibility="Visible" >
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                </Grid.RowDefinitions>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                    <ColumnDefinition ></ColumnDefinition>
                                                </Grid.ColumnDefinitions>


                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Collection Date" Margin="4" Grid.Row="1" Grid.Column="0"></TextBlock>
                                                <controls:DatePicker HorizontalAlignment="Stretch" Margin="4" Name="dtCollection" SelectedDate="{Binding CollectionDate,Mode=TwoWay}" VerticalAlignment="Top" Grid.Row="1" Grid.Column="1" />
                                                <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="Method Of Collection" Margin="4" Grid.Row="3" Grid.Column="0"></TextBlock>
                                                <CustomCombo:AutoCompleteComboBox Margin="4"  Grid.Row="3" Grid.Column="1"  x:Name="cmbCollection" SelectedValue="{Binding MethodOfCollectionID,Mode=TwoWay}" VerticalAlignment="Center" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID" ></CustomCombo:AutoCompleteComboBox>
                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Abstinence " Margin="4" Grid.Row="4" Grid.Column="0"></TextBlock>
                                                <CustomCombo:AutoCompleteComboBox Margin="4"  Grid.Row="4" Grid.Column="1"  x:Name="cmbAbstience" SelectedValue="{Binding AbstinenceID,Mode=TwoWay}" VerticalAlignment="Center" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID" ></CustomCombo:AutoCompleteComboBox>
                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Collection Time" Margin="4" Grid.Row="5" Grid.Column="0"></TextBlock>
                                                <inputToolkit:TimePicker  x:Name="CollectionTime" Value="{Binding CollectionDate, Mode=TwoWay}" Margin="2" Grid.Row="5"  Grid.Column="1" HorizontalAlignment="Stretch" VerticalAlignment="Center" />

                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Receiving Lab Time" Margin="4" Grid.Row="6" Grid.Column="0"></TextBlock>
                                                <inputToolkit:TimePicker  x:Name="SampRecTime" Value="{Binding TimeRecSampLab, Mode=TwoWay}" Margin="2" Grid.Row="6"  Grid.Column="1" HorizontalAlignment="Stretch" VerticalAlignment="Center" />

                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Complete" Margin="4" Grid.Row="7" Grid.Column="0" Visibility="Collapsed"></TextBlock>
                                                <StackPanel Grid.Row="7" Grid.Column="1" Orientation="Horizontal">
                                                    <RadioButton Content="Yes"  Height="16" HorizontalAlignment="Left" Margin="4" Name="rbtYes" VerticalAlignment="Top" Visibility="Collapsed"  />
                                                    <RadioButton Content="No"  Height="16" HorizontalAlignment="Left" Margin="4" Name="rbtNo" VerticalAlignment="Top" Visibility="Collapsed" />
                                                </StackPanel>


                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Sample Collected" Margin="4" Grid.Row="8" Grid.Column="0"></TextBlock>
                                                <StackPanel Grid.Row="8" Grid.Column="1" Orientation="Horizontal">
                                                    <RadioButton Content="At Centre"  Height="16" HorizontalAlignment="Left" Margin="4" Name="rbtCentre" VerticalAlignment="Top"  />
                                                    <RadioButton Content="Brought from Outside"  Height="16" HorizontalAlignment="Left" Margin="4" Name="rbtOutSideCentre" VerticalAlignment="Top"  />
                                                </StackPanel>
                                            </Grid >
                                        </Grp:GroupBox>

                                        <Grp:GroupBox IsTabStop="False" x:Name="grpPhysicalExam" Header="Physical Examination" Height="Auto" Grid.Row="1" Grid.Column="1" Margin="4" >
                                            <Grid  Background="White"  Visibility="Visible" >
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                </Grid.RowDefinitions>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                    <ColumnDefinition ></ColumnDefinition>
                                                </Grid.ColumnDefinitions>
                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Color" Margin="4" Grid.Row="1" Grid.Column="0"></TextBlock>
                                                <CustomCombo:AutoCompleteComboBox Margin="4"  Grid.Row="1" Grid.Column="1"  x:Name="cmbColor" SelectedValue="{Binding ColorID,Mode=TwoWay}" VerticalAlignment="Center" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID" ></CustomCombo:AutoCompleteComboBox>
                                                <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right"  Text="Volume" Margin="4" Grid.Row="2" Grid.Column="0"></TextBlock>
                                                <Grid Grid.Row="2" Grid.Column="1">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition></RowDefinition>
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition></ColumnDefinition>
                                                        <ColumnDefinition Width="auto"></ColumnDefinition>
                                                    </Grid.ColumnDefinitions>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" Name="TxtQuantity"  Text="{Binding Quantity,Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="0" Grid.Column="0" TextAlignment="Right" TextChanged="txtFloatNumber_TextChanged"  KeyDown="txtFloatNumber_KeyDown" LostFocus="TxtQuantity_LostFocus" MaxLength="20"/>
                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{Binding VolumeRange,Mode=TwoWay}" Margin="4" Grid.Row="0" Grid.Column="1"></TextBlock>

                                                </Grid>

                                                <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="PH" Margin="4" Grid.Row="3" Grid.Column="0"></TextBlock>
                                                <Grid Grid.Row="3" Grid.Column="1">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition></RowDefinition>
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition></ColumnDefinition>
                                                        <ColumnDefinition Width="auto"></ColumnDefinition>
                                                    </Grid.ColumnDefinitions>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" Name="TxtPH"  Text="{Binding PH,Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="0" Grid.Column="0" TextAlignment="Right" TextChanged="txtFloatNumber_TextChanged"  KeyDown="txtFloatNumber_KeyDown" LostFocus="TxtPH_LostFocus" MaxLength="29" />
                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{Binding PHRange,Mode=TwoWay}" Margin="4" Grid.Row="0" Grid.Column="1"></TextBlock>

                                                </Grid>


                                                <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="Liquification Time" Margin="4" Grid.Row="4" Grid.Column="0"></TextBlock>
                                                <!--<inputToolkit:TimePicker TabIndex="2" x:Name="LiquiTime" Value="{Binding LiquificationTime, Mode=TwoWay}" Margin="2" Grid.Row="4"  Grid.Column="1" HorizontalAlignment="Stretch" VerticalAlignment="Center" />-->
                                                <Grid Grid.Row="4" Grid.Column="1">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition></RowDefinition>
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition></ColumnDefinition>
                                                        <ColumnDefinition Width="auto"></ColumnDefinition>
                                                    </Grid.ColumnDefinitions>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" Name="TxtLiquiTime"  Text="{Binding LiquificationTime,Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="0" Grid.Column="0" TextAlignment="Right" LostFocus="TxtLiquiTime_LostFocus" MaxLength="29" />
                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="30 min - Normal" Margin="4" Grid.Row="0" Grid.Column="1"></TextBlock>

                                                </Grid>




                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Spillage" Margin="4" Grid.Row="5" Grid.Column="0" ></TextBlock>
                                                <TextBox  HorizontalAlignment="Stretch" Margin="4" Name="TxtSpillage" Text="{Binding Spillage,Mode=TwoWay}" VerticalAlignment="Center"   Grid.Row="5" Grid.Column="1"  TextAlignment="Right" MaxLength="29"/>
                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Fructose" Margin="4" Grid.Row="6" Grid.Column="0" ></TextBlock>
                                                <TextBox  HorizontalAlignment="Stretch" Margin="4" Name="TxtFructose" Text="{Binding Fructose,Mode=TwoWay}" VerticalAlignment="Center"   Grid.Row="6" Grid.Column="1"  TextAlignment="Right" MaxLength="29"/>



                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Live" Margin="4" Grid.Row="7" Grid.Column="0" ></TextBlock>
                                                <TextBox  HorizontalAlignment="Stretch" Margin="4" Name="TxtLive" Text="{Binding Live,Mode=TwoWay}" VerticalAlignment="Center"   Grid.Row="7" Grid.Column="1"  TextAlignment="Right" MaxLength="29" />
                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Dead" Margin="4" Grid.Row="8" Grid.Column="0" ></TextBlock>
                                                <TextBox  HorizontalAlignment="Stretch" Margin="4" Name="TxtDead" Text="{Binding Dead,Mode=TwoWay}" VerticalAlignment="Center"   Grid.Row="8" Grid.Column="1"  TextAlignment="Right" MaxLength="29" />



                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Viscosity" Margin="4" Grid.Row="9" Grid.Column="0"></TextBlock>
                                                <StackPanel Grid.Row="9" Grid.Column="1" Orientation="Horizontal">
                                                    <RadioButton Content="Normal"  Height="16" HorizontalAlignment="Left" Margin="4" Name="rbtNormal" VerticalAlignment="Top"  Checked="rbtNormal_Checked" />
                                                    <RadioButton Content="Viscous"  Height="16" HorizontalAlignment="Left" Margin="4" Name="rbtViscous" VerticalAlignment="Top" Checked="rbtNormal_Checked" />
                                                    <CustomCombo:AutoCompleteComboBox Margin="4"    x:Name="cmbViscousRange" SelectedValue="{Binding RangeViscosityID,Mode=TwoWay}" VerticalAlignment="Center" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID" Visibility="Collapsed" Width="150" ></CustomCombo:AutoCompleteComboBox>
                                                    <!--<CustomCombo:AutoCompleteComboBox Margin="4"  Grid.Row="4" Grid.Column="1"  x:Name="cmbAbstience" SelectedValue="{Binding AbstinenceID,Mode=TwoWay}" VerticalAlignment="Center" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID" ></CustomCombo:AutoCompleteComboBox>-->
                                                </StackPanel>
                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Odour" Margin="4" Grid.Row="10" Grid.Column="0"></TextBlock>
                                                <StackPanel Grid.Row="10" Grid.Column="1" Orientation="Horizontal">
                                                    <RadioButton Content="Present"  Height="16" HorizontalAlignment="Left" Margin="4" Name="rbtPresent" VerticalAlignment="Top"  />
                                                    <RadioButton Content="Not Present"  Height="16" HorizontalAlignment="Left" Margin="4" Name="rbtNotPresent" VerticalAlignment="Top"  />
                                                </StackPanel>
                                            </Grid>
                                        </Grp:GroupBox>

                                        <Grp:GroupBox IsTabStop="False" x:Name="grpMicroscopicExam" Header="Microscopic Examination" Height="Auto" Grid.Row="2" Grid.Column="0" Margin="4" >
                                            <Grid  Background="White"  Visibility="Visible" >
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                </Grid.RowDefinitions>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="Auto" ></ColumnDefinition>
                                                    <ColumnDefinition Width="40"></ColumnDefinition>
                                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                    <ColumnDefinition ></ColumnDefinition>
                                                </Grid.ColumnDefinitions>
                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Sperm Concentration" Margin="4" Grid.Row="1" Grid.Column="0" ></TextBlock>
                                                <TextBox  HorizontalAlignment="Stretch" Margin="4" Name="TxtSpermCount" Text="{Binding SpermCount,Mode=TwoWay}" VerticalAlignment="Center"   Grid.Row="1" Grid.Column="1"  TextAlignment="Right" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" LostFocus="TxtSpermCount_LostFocus"  MaxLength="29"  />
                                                <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Left" Text="&gt; 15 mill/ml" Margin="4" Grid.Row="1" Grid.Column="2"></TextBlock>
                                                <Grid Grid.Row="1" Grid.Column="3">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition></RowDefinition>
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="auto"></ColumnDefinition>
                                                        <ColumnDefinition Width="80"></ColumnDefinition>
                                                        <ColumnDefinition Width="auto"></ColumnDefinition>
                                                        <ColumnDefinition Width="auto"></ColumnDefinition>
                                                        <ColumnDefinition Width="auto"></ColumnDefinition>
                                                    </Grid.ColumnDefinitions>                                                   
                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="5th Percentile :" Margin="4" Grid.Row="0" Grid.Column="0"></TextBlock>
                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="left" Text=">15 mill/ml" Margin="4" Grid.Row="0" Grid.Column="1"></TextBlock>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" Name="TxtSperm5thPercentile" Text="{Binding Sperm5thPercentile,Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="0" Grid.Column="1"  TextAlignment="Right" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="29" Visibility="Collapsed"/>
                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="75th Percentile :" Margin="4" Grid.Row="0" Grid.Column="2"></TextBlock>
                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="left" Text=">40 million" Margin="4" Grid.Row="0" Grid.Column="3"></TextBlock>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" Name="TxtSperm75thPercentile" Text="{Binding Sperm75thPercentile,Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="0" Grid.Column="3"  TextAlignment="Right" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="29" Visibility="Collapsed" />
                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="- Normal" Margin="4" Grid.Row="0" Grid.Column="4"></TextBlock>


                                                    <!--<TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="5th Percentile" Margin="4" Grid.Row="0" Grid.Column="0"></TextBlock>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" Name="TxtSperm5thPercentile" Text="{Binding Sperm5thPercentile,Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="0" Grid.Column="1"  TextAlignment="Right" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="29" />
                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="75th Percentile" Margin="4" Grid.Row="0" Grid.Column="2"></TextBlock>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" Name="TxtSperm75thPercentile" Text="{Binding Sperm75thPercentile,Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="0" Grid.Column="3"  TextAlignment="Right" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="29" />
                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="- Normal" Margin="4" Grid.Row="0" Grid.Column="4"></TextBlock>-->
                                                </Grid>

                                                <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="Total Ejaculate" Margin="4" Grid.Row="2" Grid.Column="0"></TextBlock>
                                                <TextBox  HorizontalAlignment="Stretch" Margin="4" Name="TxtTotCount" Text="{Binding TotalSpermCount,Mode=TwoWay}" VerticalAlignment="Center"   Grid.Row="2" Grid.Column="1"  TextAlignment="Right" LostFocus="TxtTotCount_LostFocus" MaxLength="29" IsEnabled="False"/>
                                                <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="&gt;= 39 millions" Margin="4" Grid.Row="2" Grid.Column="2"></TextBlock>
                                                <Grid Grid.Row="2" Grid.Column="3">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition></RowDefinition>
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="auto"></ColumnDefinition>
                                                        <ColumnDefinition Width="80"></ColumnDefinition>
                                                        <ColumnDefinition Width="auto"></ColumnDefinition>
                                                        <ColumnDefinition Width="auto"></ColumnDefinition>
                                                        <ColumnDefinition Width="auto"></ColumnDefinition>
                                                    </Grid.ColumnDefinitions>

                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="5th Percentile :" Margin="4" Grid.Row="0" Grid.Column="0"></TextBlock>
                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="left" Text=">39 mill" Margin="4" Grid.Row="0" Grid.Column="1"></TextBlock>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" Name="TxtEjaculate5thPercentile" Text="{Binding Ejaculate5thPercentile,Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="0" Grid.Column="1"  TextAlignment="Right" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="29" Visibility="Collapsed"/>
                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="75th Percentile :" Margin="4" Grid.Row="0" Grid.Column="2"></TextBlock>
                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Left" Text=">120 million" Margin="4" Grid.Row="0" Grid.Column="3"></TextBlock>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" Name="TxtEjaculate75thPercentile" Text="{Binding Ejaculate75thPercentile,Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="0" Grid.Column="3"  TextAlignment="Right" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="29" Visibility="Collapsed"/>
                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="- Normal" Margin="4" Grid.Row="0" Grid.Column="4"></TextBlock>


                                                    <!--<TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="5th Percentile" Margin="4" Grid.Row="0" Grid.Column="0"></TextBlock>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" Name="TxtEjaculate5thPercentile" Text="{Binding Ejaculate5thPercentile,Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="0" Grid.Column="1"  TextAlignment="Right" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="29" />
                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="75th Percentile" Margin="4" Grid.Row="0" Grid.Column="2"></TextBlock>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" Name="TxtEjaculate75thPercentile" Text="{Binding Ejaculate75thPercentile,Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="0" Grid.Column="3"  TextAlignment="Right" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="29" />
