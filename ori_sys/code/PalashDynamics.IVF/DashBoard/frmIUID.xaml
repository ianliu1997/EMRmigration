                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" x:Name="TxtPreGradeI" Text="{Binding PreMotilityGradeI,Mode=TwoWay}" VerticalAlignment="Center"   Grid.Row="6" Grid.Column="1" TextAlignment="Right" TextChanged="PreTotal_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="29" Visibility="Collapsed"/>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" x:Name="TxtPostGradeI" Text="{Binding PostMotilityGradeI,Mode=TwoWay}" VerticalAlignment="Center"   Grid.Row="6" Grid.Column="2" TextAlignment="Right" TextChanged="PostTotal_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="29" Visibility="Collapsed" />

                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="Grade II" Margin="4" Grid.Row="7" Grid.Column="0" Visibility="Collapsed"></TextBlock>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" x:Name="TxtPreGradeII" Text="{Binding PreMotilityGradeII,Mode=TwoWay}" VerticalAlignment="Center"   Grid.Row="7" Grid.Column="1" TextAlignment="Right" TextChanged="PreTotal_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="29" Visibility="Collapsed"/>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" x:Name="TxtPostGradeII" Text="{Binding PostMotilityGradeII,Mode=TwoWay}" VerticalAlignment="Center"   Grid.Row="7" Grid.Column="2" TextAlignment="Right" TextChanged="PostTotal_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="29" Visibility="Collapsed"/>

                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="Grade III" Margin="4" Grid.Row="8" Grid.Column="0" Visibility="Collapsed"></TextBlock>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" x:Name="TxtPreGradeIII" Text="{Binding PreMotilityGradeIII,Mode=TwoWay}" VerticalAlignment="Center"   Grid.Row="8" Grid.Column="1" TextAlignment="Right" TextChanged="PreTotal_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="29" Visibility="Collapsed"/>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" x:Name="TxtPostGradeIII" Text="{Binding PostMotilityGradeIII,Mode=TwoWay}" VerticalAlignment="Center"   Grid.Row="8" Grid.Column="2" TextAlignment="Right" TextChanged="PostTotal_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="29" Visibility="Collapsed"/>

                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="Grade IV" Margin="4" Grid.Row="9" Grid.Column="0" Visibility="Collapsed"></TextBlock>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" x:Name="TxtPreGradeIV"  Text="{Binding PreMotilityGradeIV,Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="9" Grid.Column="1" TextAlignment="Right" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="29"  IsEnabled="False" Visibility="Collapsed"/>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" x:Name="TxtPostGradeIV"  Text="{Binding PostMotilityGradeIV,Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="9" Grid.Column="2" TextAlignment="Right" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="29"  IsEnabled="False" Visibility="Collapsed"/>

                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="Morphology" Margin="4" Grid.Row="10" Grid.Column="0" Visibility="Collapsed"></TextBlock>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" x:Name="TxtPreNormalMorphology"  Text="{Binding PreNormalMorphology,Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="10" Grid.Column="1" TextAlignment="Right" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="29" Visibility="Collapsed"/>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" x:Name="TxtPostNormalMorphology"  Text="{Binding PostNormalMorphology,Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="10" Grid.Column="2" TextAlignment="Right" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="29" Visibility="Collapsed"/>





                                                </Grid >
                                            </Grp:GroupBox>

                                            <Grp:GroupBox IsTabStop="False" x:Name="grpCellInfo" Header="Cell Information" Height="Auto" Grid.Row="5" Grid.ColumnSpan="2" Margin="4" Visibility="Collapsed">
                                                <Grid  Background="White"  Visibility="Visible" >
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto"></RowDefinition>
                                                        <RowDefinition Height="Auto"></RowDefinition>
                                                        <!--<RowDefinition Height="Auto"></RowDefinition>-->
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                        <ColumnDefinition ></ColumnDefinition>
                                                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                        <ColumnDefinition ></ColumnDefinition>
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Pus Cells" Margin="4" Grid.Row="0" Grid.Column="0"></TextBlock>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" x:Name="TxtPusCells" Text="{Binding PusCells,Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="0" Grid.Column="1" TextAlignment="Left"  MaxLength="29" />
                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="RBC" Margin="4" Grid.Row="0" Grid.Column="2"></TextBlock>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" x:Name="TxtRoundCells" Text="{Binding RoundCells,Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="0" Grid.Column="3"  TextAlignment="Left"  MaxLength="29"/>

                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="Epithelial Cells" Margin="4" Grid.Row="1" Grid.Column="0"></TextBlock>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" x:Name="TxtEpithelialCells" Text="{Binding EpithelialCells,Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="1" Grid.Column="1" TextAlignment="Left"  MaxLength="29" />
                                                    <TextBlock TextAlignment="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Text="Other Cells" Margin="4" Grid.Row="1" Grid.Column="2"></TextBlock>
                                                    <TextBox  HorizontalAlignment="Stretch" Margin="4" x:Name="TxtOtherCells" Text="{Binding AnyOtherCells,Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="1" Grid.Column="3"  TextAlignment="Left"  MaxLength="29"/>
                                                </Grid>
                                            </Grp:GroupBox>

                                        </Grid>

                                    </Border>
                                </ScrollViewer>
                            </controlsToolkit:DockPanel>
                        </Grid>
                    </Grid>
                </Border>

            </controlsToolkit:DockPanel>
        </Grid>

    </Grid>
