<UserControl x:Class="PalashDynamics.IVF.DashBoard.frmCryoPreservation"
            xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"       
            xmlns:controls="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls"     
            xmlns:blacklight="clr-namespace:Blacklight.Silverlight.Controls;assembly=Blacklight.Silverlight.Controls"
            xmlns:local="clr-namespace:PalashDynamics.Converters;assembly=PalashDynamics.Converters"  
            xmlns:controlsToolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"   
            xmlns:inputToolkit="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Input.Toolkit"                  
            xmlns:dataInput="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Data.Input"
            xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"     
            xmlns:liquid="clr-namespace:Liquid;assembly=Liquid"
            xmlns:data="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"      
            xmlns:input="clr-namespace:PalashDynamics.Controls;assembly=PalashDynamics.Controls.AutoCompleteComboBox"     
            xmlns:inputcombo="clr-namespace:PalashDynamics.Controls;assembly=PalashDynamics.Controls.AutoCompleteComboBox"
            xmlns:Grp ="clr-namespace:Groupbox;assembly=Groupbox"  
            xmlns:Common="clr-namespace:DataDrivenApplication"   Loaded="UserControl_Loaded"  >

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/PalashDynamics;component/Generic.xaml"/>
                <ResourceDictionary Source="Themes/Generic1.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <local:DateTimeConverter x:Key="DateConverter" />
            <local:DateTimeToAgePart x:Key="DateTimeToAgePart"></local:DateTimeToAgePart>
        </ResourceDictionary>
    </UserControl.Resources>


    <Grid Background="White">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <!--<RowDefinition Height="Auto"/>-->
        </Grid.RowDefinitions>

        <Grid Grid.Row="1" Grid.RowSpan="2" Grid.Column="0" >
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>

            <controlsToolkit:DockPanel controlsToolkit:DockPanel.Dock="Bottom" >
                <controls:TabControl Grid.Row="0" Padding="0"  x:Name="tabControl2" Margin="4,-4,4,4" SelectionChanged="tabControl1_SelectionChanged" ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Auto">

                    <controls:TabItem  x:Name="OVitrification" Style="{StaticResource TabItemStyle}" ScrollViewer.VerticalScrollBarVisibility="Auto" Padding="0" Margin="0" TabIndex="1" Header="Oocyte Vitrification" Visibility="Visible">
                        <Border  BorderBrush="{StaticResource TabControlDarkerBorder}" toolkit:DockPanel.Dock="Top" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0,-1,0,0" BorderThickness="1" CornerRadius="0,0,3,3" Background="Transparent">
                            <Grid ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Auto">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                </Grid.RowDefinitions>
                                <StackPanel Grid.Column="0" Grid.Row="0">
                                    <Grid controlsToolkit:DockPanel.Dock="Top" x:Name="OcVitrificationMainGrid">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                        </Grid.RowDefinitions>
                                        <TextBlock Text="Vitrification Number" Grid.Row="0" Grid.Column="0" Margin="4" VerticalAlignment="Center"></TextBlock>
                                        <TextBox Text="{Binding VitrificationNo,Mode=TwoWay}" Grid.Row="0" Grid.Column="1" Margin="4" x:Name="txtOcVitriNo" IsEnabled="False"></TextBox>
                                        <TextBlock Text="Vitrification Date" Grid.Row="0" Grid.Column="2" Margin="4" VerticalAlignment="Center"></TextBlock>
                                        <controls:DatePicker  Grid.Row="0" Grid.Column="3"  SelectedDate="{Binding DateTime,Mode=TwoWay}"  Margin="4"  x:Name="dtOcVitrificationDate" LostFocus="dtOcVitrificationDate_LostFocus"  ></controls:DatePicker>

                                        <TextBlock Text="Vitrification Time" Grid.Row="0" Grid.Column="4" Margin="4" VerticalAlignment="Center"></TextBlock>
                                        <inputToolkit:TimePicker Margin="4"  Grid.Row="0" Grid.Column="5" Value="{Binding DateTime,Mode=TwoWay}"  x:Name="txtOcTime" LostFocus="dtOcVitrificationDate_LostFocus"></inputToolkit:TimePicker>

                                        <TextBlock Text="Expiry Date" Grid.Row="0" Grid.Column="6" Margin="4" VerticalAlignment="Center"></TextBlock>
                                        <controls:DatePicker  Grid.Row="0" Grid.Column="7"  SelectedDate="{Binding ExpiryDate,Mode=TwoWay}"  Margin="4"  x:Name="dtOcExpiryDate"></controls:DatePicker>

                                        <TextBlock Text="Expiry Time" Grid.Row="0" Grid.Column="8" Margin="4" VerticalAlignment="Center"></TextBlock>
                                        <inputToolkit:TimePicker Margin="4"  Grid.Row="0" Grid.Column="9" Value="{Binding ExpiryDate,Mode=TwoWay}"  x:Name="txtOcExpiryTime"></inputToolkit:TimePicker>


                                        <TextBlock Text="OPU Date" Grid.Row="2" Grid.Column="0" Margin="4" VerticalAlignment="Center" Visibility="Collapsed"></TextBlock>
                                        <controls:DatePicker  Grid.Row="2" Grid.Column="1"  SelectedDate="{Binding dtPickUpDate ,Mode=TwoWay}"    Margin="4"  x:Name="dtOcPickUpDate" Visibility="Collapsed"></controls:DatePicker>
                                        <!--<CheckBox x:Name="chkOcFreeze" Content="Freeze" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="4" FontWeight="ExtraBlack" Foreground="Red"  Grid.Row="0" Grid.Column="2"  IsChecked="{Binding IsFreezed,Mode=TwoWay}"  TabIndex="12"></CheckBox>-->
                                    </Grid>
                                </StackPanel>

                                <!--<StackPanel Grid.Column="0" Grid.Row="0">
                                    <Grid controlsToolkit:DockPanel.Dock="Top" x:Name="OcVitrificationMainGrid">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                        </Grid.RowDefinitions>
                                        <TextBlock Text="Vitrification Number" Grid.Row="0" Grid.Column="0" Margin="4" VerticalAlignment="Center"></TextBlock>
                                        <TextBox Text="{Binding VitrificationNo,Mode=TwoWay}" Grid.Row="0" Grid.Column="1" Margin="4" x:Name="txtOcVitriNo" IsEnabled="False"></TextBox>
                                        <TextBlock Text="Vitrification Date" Grid.Row="0" Grid.Column="2" Margin="4" VerticalAlignment="Center"></TextBlock>
                                        <controls:DatePicker  Grid.Row="0" Grid.Column="3"  SelectedDate="{Binding DateTime,Mode=TwoWay}"  Margin="4"  x:Name="dtOcVitrificationDate"></controls:DatePicker>

                                        <TextBlock Text="Vitrification Time" Grid.Row="0" Grid.Column="4" Margin="4" VerticalAlignment="Center"></TextBlock>
                                        <inputToolkit:TimePicker Margin="4"  Grid.Row="0" Grid.Column="5" Value="{Binding DateTime,Mode=TwoWay}"  x:Name="txtOcTime"></inputToolkit:TimePicker>

                                        <TextBlock Grid.Row="1" Grid.Column="0" Text="Embryologist" Margin="2" VerticalAlignment="Center"></TextBlock>
                                        <inputcombo:AutoCompleteComboBox  TabIndex="3" x:Name="cmbEmbryologistOocyte" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID" VerticalAlignment="Center" MinWidth="100"  Margin="2"  Grid.Row="1" Grid.Column="1" ></inputcombo:AutoCompleteComboBox>
                                        <TextBlock Grid.Row="1" Grid.Column="2" Text="Assistant Embryologist" Margin="2" VerticalAlignment="Center"></TextBlock>
                                        <inputcombo:AutoCompleteComboBox  TabIndex="4" x:Name="cmbAssistantEmbryologistOocyte" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID" VerticalAlignment="Center" MinWidth="100" Margin="2"  Grid.Row="1" Grid.Column="3"  ></inputcombo:AutoCompleteComboBox>

                                        <TextBlock Text="OPU Date" Grid.Row="2" Grid.Column="0" Margin="4" VerticalAlignment="Center" Visibility="Collapsed"></TextBlock>
                                        <controls:DatePicker  Grid.Row="2" Grid.Column="1"  SelectedDate="{Binding dtPickUpDate ,Mode=TwoWay}"    Margin="4"  x:Name="dtOcPickUpDate" Visibility="Collapsed"></controls:DatePicker>
                                        <CheckBox x:Name="chkOcFreeze" Content="Freeze" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="4" FontWeight="ExtraBlack" Foreground="Red"  Grid.Row="1" Grid.Column="4"  IsChecked="{Binding IsFreezed,Mode=TwoWay}"  TabIndex="12"></CheckBox>

                                    </Grid>
                                </StackPanel>-->

                                <StackPanel Grid.Column="0" Grid.Row="1">
                                    <controlsToolkit:DockPanel LastChildFill="True" controlsToolkit:DockPanel.Dock="Bottom">
                                        <Border controlsToolkit:DockPanel.Dock="Top"   BorderBrush="{StaticResource TabControlDarkerBorder}"   BorderThickness="1" Margin="0,4,0,0" CornerRadius="3,3,0,0"  Background="Transparent" >
                                            <ContentControl VerticalAlignment="Center" FontSize="12" controlsToolkit:DockPanel.Dock="Top" Content="Oocyte Vitrification" Margin="-1" Style="{StaticResource Header}"     HorizontalAlignment="Stretch" />
                                        </Border>
                                        <controlsToolkit:DockPanel LastChildFill="True"  controlsToolkit:DockPanel.Dock="Bottom">
                                            <data:DataPager x:Name="OcyVitrificationDetilsGridPager" controlsToolkit:DockPanel.Dock="Bottom" DisplayMode="FirstLastPreviousNextNumeric"   Source="{Binding}" NumericButtonCount="10" AutoEllipsis="False" IsTotalItemCountFixed="True" ScrollViewer.HorizontalScrollBarVisibility="Visible" />
                                            <data:DataGrid  SelectionMode="Single" x:Name="dgOcyVitrificationDetilsGrid" ItemsSource="{Binding}"  AutoGenerateColumns="False" Width="Auto" Height="225">
                                                <data:DataGrid.Columns>
                                                    <data:DataGridTextColumn Width="80" Binding="{Binding EmbSerialNumber,Mode=TwoWay}" Header="EmbSerialNumber" IsReadOnly="True" Visibility="Collapsed"/>
                                                    <data:DataGridTextColumn Width="80" Binding="{Binding EmbNumber,Mode=TwoWay}" Header="Oocyte No" IsReadOnly="True"/>
                                                    <data:DataGridTemplateColumn Header="Tank">
                                                        <data:DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <Grid>
                                                                    <input:AutoCompleteComboBox x:Name="cmbTankId"  Margin="4" ItemsSource="{Binding TankList}"   SelectedItem="{Binding SelectedTank,Mode=TwoWay}"  SelectedValue="{Binding SelectedTank.ID}" VerticalAlignment="Top" Text="" SelectionChanged="cmbComboOocyte_SelectionChanged"/>
                                                                </Grid>
                                                            </DataTemplate>
                                                        </data:DataGridTemplateColumn.CellTemplate>
                                                    </data:DataGridTemplateColumn>
                                                    <data:DataGridTemplateColumn Header="Canister">
                                                        <data:DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <Grid>
                                                                    <input:AutoCompleteComboBox x:Name="cmbCanisterId"  Margin="4" ItemsSource="{Binding CanisterIdList}"   SelectedItem="{Binding SelectedCanisterId,Mode=TwoWay}"  SelectedValue="{Binding SelectedCanisterId.ID}" VerticalAlignment="Top" Text="" SelectionChanged="cmbComboOocyte_SelectionChanged"/>
                                                                </Grid>
                                                            </DataTemplate>
                                                        </data:DataGridTemplateColumn.CellTemplate>
                                                    </data:DataGridTemplateColumn>
                                                    <data:DataGridTemplateColumn Header="Cane ">
                                                        <data:DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <Grid>
                                                                    <input:AutoCompleteComboBox x:Name="cmbCanID"  Margin="4" ItemsSource="{Binding CanIdList}"   SelectedItem="{Binding SelectedCanId,Mode=TwoWay}"  SelectedValue="{Binding SelectedCanId.ID}" VerticalAlignment="Top" Text="" SelectionChanged="cmbComboOocyte_SelectionChanged"/>
                                                                </Grid>
                                                            </DataTemplate>
                                                        </data:DataGridTemplateColumn.CellTemplate>
                                                    </data:DataGridTemplateColumn>
                                                    <data:DataGridTemplateColumn Header="Goblet Color">
                                                        <data:DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <Grid>
                                                                    <input:AutoCompleteComboBox x:Name="cmbColorSelector"  Margin="4" ItemsSource="{Binding ColorSelectorList}"   SelectedItem="{Binding SelectedColorSelector,Mode=TwoWay}"  SelectedValue="{Binding SelectedColorSelector.ID}" VerticalAlignment="Top" Text="" SelectionChanged="cmbComboOocyte_SelectionChanged"/>
                                                                </Grid>
                                                            </DataTemplate>
                                                        </data:DataGridTemplateColumn.CellTemplate>
                                                    </data:DataGridTemplateColumn>
                                                    <data:DataGridTemplateColumn Header="Straw">
                                                        <data:DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <Grid>
                                                                    <input:AutoCompleteComboBox x:Name="cmbStraw"  Margin="4" ItemsSource="{Binding StrawIdList}"   SelectedItem="{Binding SelectedStrawId,Mode=TwoWay}"  SelectedValue="{Binding SelectedStrawId.ID}" VerticalAlignment="Top" Text="" SelectionChanged="cmbComboOocyte_SelectionChanged"/>
                                                                </Grid>
                                                            </DataTemplate>
                                                        </data:DataGridTemplateColumn.CellTemplate>
                                                    </data:DataGridTemplateColumn>
                                                    <data:DataGridTemplateColumn Header="Goblet Shape" Visibility="Collapsed">
                                                        <data:DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <Grid>
                                                                    <input:AutoCompleteComboBox x:Name="cmbGobletShape"  Margin="4" ItemsSource="{Binding GobletShapeList}"   SelectedItem="{Binding SelectedGobletShape,Mode=TwoWay}"  SelectedValue="{Binding SelectedGobletShape.ID}" VerticalAlignment="Top" Text="" SelectionChanged="cmbComboOocyte_SelectionChanged"/>
                                                                </Grid>
                                                            </DataTemplate>
                                                        </data:DataGridTemplateColumn.CellTemplate>
                                                    </data:DataGridTemplateColumn>                                                   
                                                    <data:DataGridTemplateColumn Header="Goblet Size" Visibility="Collapsed">
                                                        <data:DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <Grid>
                                                                    <input:AutoCompleteComboBox x:Name="cmbGobletSize"  Margin="4" ItemsSource="{Binding GobletSizeList}"   SelectedItem="{Binding SelectedGobletSize,Mode=TwoWay}"  SelectedValue="{Binding SelectedGobletSize.ID}" VerticalAlignment="Top" Text="" SelectionChanged="cmbComboOocyte_SelectionChanged"/>
                                                                </Grid>
                                                            </DataTemplate>
                                                        </data:DataGridTemplateColumn.CellTemplate>
                                                    </data:DataGridTemplateColumn>  

                                                    <data:DataGridTextColumn Width="100" Binding="{Binding CellStage,Mode=TwoWay}" Header="Cell Stage" IsReadOnly="True" Visibility="Collapsed"/>

                                                    <data:DataGridTextColumn  Width="100" Binding="{Binding Grade,Mode=TwoWay}" Header="Grade" IsReadOnly="True" Visibility="Collapsed"/>

                                                    <data:DataGridTemplateColumn Header="Oocyte Maturity" >
                                                        <data:DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <Grid>
                                                                    <input:AutoCompleteComboBox x:Name="cmbOocyteGrade"  Margin="4" ItemsSource="{Binding OocyteGradeList}"   SelectedItem="{Binding SelectedOocyteGrade,Mode=TwoWay}"  SelectedValue="{Binding SelectedOocyteGrade.ID}" VerticalAlignment="Top" Text=""  SelectionChanged="cmbOocyteGrade_SelectionChanged"/>
                                                                </Grid>
                                                            </DataTemplate>
                                                        </data:DataGridTemplateColumn.CellTemplate>
                                                    </data:DataGridTemplateColumn>


                                                    <data:DataGridTemplateColumn Header="Oocyte Status" Visibility="Collapsed">
                                                        <data:DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <Grid>
                                                                    <TextBox AcceptsReturn="True"  Margin="4" TextWrapping="Wrap" Width="100" Height="28" Text="{Binding EmbStatus,Mode=TwoWay}"></TextBox>
                                                                </Grid>
                                                            </DataTemplate>
                                                        </data:DataGridTemplateColumn.CellTemplate>
                                                    </data:DataGridTemplateColumn>
                                                    <data:DataGridTextColumn Width="100" Binding="{Binding TransferDay,Mode=TwoWay}" Header="Plan Day" IsReadOnly="True"/>

                                                    <data:DataGridTextColumn Binding="{Binding TransferDate,Mode=TwoWay}" Header="Plan Date" IsReadOnly="True"/>
                                                    <data:DataGridTemplateColumn Header="Comments">
                                                        <data:DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <Grid>
                                                                    <TextBox AcceptsReturn="True"  Margin="4" TextWrapping="Wrap" Width="100" Height="28" Text="{Binding Comments,Mode=TwoWay}" ></TextBox>
                                                                </Grid>
                                                            </DataTemplate>
                                                        </data:DataGridTemplateColumn.CellTemplate>
                                                    </data:DataGridTemplateColumn>

                                                    <!--added by neena-->
                                                    <data:DataGridTemplateColumn Header="Vitrification Date" Visibility="Collapsed">
                                                        <data:DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <Grid>
                                                                    <controls:DatePicker  Width="100" Height="28"  SelectedDate="{Binding VitrificationDate,Mode=TwoWay}"  Margin="4" ></controls:DatePicker>
                                                                </Grid>
                                                            </DataTemplate>
                                                        </data:DataGridTemplateColumn.CellTemplate>
                                                    </data:DataGridTemplateColumn>

                                                    <data:DataGridTemplateColumn Header="Vitrification Time" Visibility="Collapsed">
                                                        <data:DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <Grid>
                                                                    <inputToolkit:TimePicker Margin="4" Width="100" Height="28" Value="{Binding VitrificationTime,Mode=TwoWay}" ></inputToolkit:TimePicker>
                                                                </Grid>
                                                            </DataTemplate>
                                                        </data:DataGridTemplateColumn.CellTemplate>
                                                    </data:DataGridTemplateColumn>

                                                    <data:DataGridTemplateColumn Header="Vitrification Number" Visibility="Collapsed">
                                                        <data:DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <Grid>
                                                                    <TextBox Text="{Binding VitrificationNo,Mode=TwoWay}" Width="150" Height="28" Margin="4" IsEnabled="False"></TextBox>

                                                                </Grid>
                                                            </DataTemplate>
                                                        </data:DataGridTemplateColumn.CellTemplate>
                                                    </data:DataGridTemplateColumn>

                                                    <data:DataGridTemplateColumn Visibility="Collapsed">
                                                        <data:DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <Grid>
                                                                    <ToggleButton Background="#000000" Content="Save"  Margin="4" Width="70" Height="28" HorizontalAlignment="Stretch" Style="{StaticResource ToggleButtonStyle}" Click="ToggleButton_Click_1" IsEnabled="{Binding IsEnabled,Mode=TwoWay}">

                                                                    </ToggleButton>
                                                                </Grid>
                                                            </DataTemplate>
                                                        </data:DataGridTemplateColumn.CellTemplate>
                                                    </data:DataGridTemplateColumn>


                                                   
                                                </data:DataGrid.Columns>
                                            </data:DataGrid>
                                        </controlsToolkit:DockPanel>
                                    </controlsToolkit:DockPanel>
                                </StackPanel>
                                <controlsToolkit:DockPanel x:Name="docFooterOcy" controlsToolkit:DockPanel.Dock="Bottom" Grid.Row="2">
                                    <controlsToolkit:DockPanel Margin="2" controlsToolkit:DockPanel.Dock="Top">
                                        <StackPanel x:Name="FooterBackPanelOcy" Orientation="Horizontal"    Margin="0,3,5,3" HorizontalAlignment="Right" Background="Transparent">
                                            <StackPanel x:Name="SaveCommandPanelOcy" Orientation="Horizontal"  Margin="0,0,0,0">
                                                <CheckBox x:Name="chkOcFreeze" Content="Finalize" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="4" FontWeight="ExtraBlack" Foreground="Red"  Grid.Row="0" Grid.Column="2"  IsChecked="{Binding IsFreezed,Mode=TwoWay}"  TabIndex="12"></CheckBox>

                                                <ToggleButton x:Name="CmdmediaOcy"  Content="Media Details" Width="100" Background="#000000" Margin="5,0,0,0" HorizontalAlignment="Stretch" Style="{StaticResource ToggleButtonStyle}" Click="Cmdmedia_Click" Visibility="Collapsed"/>
                                                <ToggleButton x:Name="cmdNewOcy"  Background="#000000" Margin="5,0,0,0" HorizontalAlignment="Stretch" Style="{StaticResource ToggleButtonStyle}" Visibility="Visible" Click="cmdSaveOocyte_Click" TabIndex="3">
                                                    <StackPanel Orientation="Horizontal" >
                                                        <Common:XamlCheckmark Width="10" Height="10" Margin="0,0,5,0"/>
                                                        <TextBlock Text="Save" TextWrapping="Wrap"/>
                                                    </StackPanel>
                                                </ToggleButton>
                                                <ToggleButton x:Name="cmdVitriCancelOcy"  Background="#000000" Margin="5,0,0,0" HorizontalAlignment="Stretch" Style="{StaticResource ToggleButtonStyle}" Visibility="Collapsed" Click="cmdCancel_Click" TabIndex="3" >
                                                    <StackPanel Orientation="Horizontal" >
                                                        <Common:XamlCross Width="10" Height="10" Margin="0,0,5,0"/>
                                                        <TextBlock Text="Cancel" TextWrapping="Wrap"/>
                                                    </StackPanel>
                                                </ToggleButton>
                                            </StackPanel>
                                        </StackPanel>
                                    </controlsToolkit:DockPanel>
                                </controlsToolkit:DockPanel>

                                <StackPanel Grid.Column="0" Grid.Row="3">
                                    <Grid controlsToolkit:DockPanel.Dock="Top" x:Name="OcRefeezeVitrificationMainGrid">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                        </Grid.RowDefinitions>
                                        <TextBlock Text="Vitrification Number" Grid.Row="0" Grid.Column="0" Margin="4" VerticalAlignment="Center"></TextBlock>
                                        <TextBox Text="{Binding VitrificationNo,Mode=TwoWay}" Grid.Row="0" Grid.Column="1" Margin="4" x:Name="txtRefeezeOcVitriNo" IsEnabled="False"></TextBox>
                                        <TextBlock Text="Refreeze Date" Grid.Row="0" Grid.Column="2" Margin="4" VerticalAlignment="Center"></TextBlock>
                                        <controls:DatePicker  Grid.Row="0" Grid.Column="3"  SelectedDate="{Binding DateTime,Mode=TwoWay}"  Margin="4"  x:Name="dtRefeezeOcVitrificationDate" LostFocus="dtRefeezeOcVitrificationDate_LostFocus"></controls:DatePicker>

                                        <TextBlock Text="Refreeze Time" Grid.Row="0" Grid.Column="4" Margin="4" VerticalAlignment="Center"></TextBlock>
                                        <inputToolkit:TimePicker Margin="4"  Grid.Row="0" Grid.Column="5" Value="{Binding DateTime,Mode=TwoWay}"  x:Name="txtRefeezeOcTime"  LostFocus="dtRefeezeOcVitrificationDate_LostFocus"></inputToolkit:TimePicker>

                                        <TextBlock Text="Expiry Date" Grid.Row="0" Grid.Column="6" Margin="4" VerticalAlignment="Center"></TextBlock>
                                        <controls:DatePicker  Grid.Row="0" Grid.Column="7"  SelectedDate="{Binding ExpiryDate,Mode=TwoWay}"  Margin="4"  x:Name="dtOcReExpiryDate"></controls:DatePicker>

                                        <TextBlock Text="Expiry Time" Grid.Row="0" Grid.Column="8" Margin="4" VerticalAlignment="Center"></TextBlock>
                                        <inputToolkit:TimePicker Margin="4"  Grid.Row="0" Grid.Column="9" Value="{Binding ExpiryDate,Mode=TwoWay}"  x:Name="txtOcReExpiryTime"></inputToolkit:TimePicker>



                                        <TextBlock Text="OPU Date" Grid.Row="2" Grid.Column="0" Margin="4" VerticalAlignment="Center" Visibility="Collapsed"></TextBlock>
                                        <controls:DatePicker  Grid.Row="2" Grid.Column="1"  SelectedDate="{Binding dtPickUpDate ,Mode=TwoWay}"    Margin="4"  x:Name="dtRefeezeOcPickUpDate" Visibility="Collapsed"></controls:DatePicker>
                                        <!--<CheckBox x:Name="chkOcFreeze" Content="Freeze" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="4" FontWeight="ExtraBlack" Foreground="Red"  Grid.Row="0" Grid.Column="2"  IsChecked="{Binding IsFreezed,Mode=TwoWay}"  TabIndex="12"></CheckBox>-->
                                    </Grid>
                                </StackPanel>

                                <StackPanel Grid.Column="0" Grid.Row="4">
                                    <controlsToolkit:DockPanel LastChildFill="True" controlsToolkit:DockPanel.Dock="Bottom">
                                        <Border controlsToolkit:DockPanel.Dock="Top"   BorderBrush="{StaticResource TabControlDarkerBorder}"   BorderThickness="1" Margin="0,4,0,0" CornerRadius="3,3,0,0"  Background="Transparent" >
                                            <ContentControl VerticalAlignment="Center" FontSize="12" controlsToolkit:DockPanel.Dock="Top" Content="Refreeze Oocyte" Margin="-1" Style="{StaticResource Header}"     HorizontalAlignment="Stretch" />
                                        </Border>
                                        <controlsToolkit:DockPanel LastChildFill="True"  controlsToolkit:DockPanel.Dock="Bottom">
                                            <data:DataPager x:Name="OcyRefeezeVitrificationDetilsGridPager" controlsToolkit:DockPanel.Dock="Bottom" DisplayMode="FirstLastPreviousNextNumeric"   Source="{Binding}" NumericButtonCount="10" AutoEllipsis="False" IsTotalItemCountFixed="True" ScrollViewer.HorizontalScrollBarVisibility="Visible" />
                                            <data:DataGrid  SelectionMode="Single" x:Name="dgRefeezeOcyVitrificationDetilsGrid" ItemsSource="{Binding}"  AutoGenerateColumns="False" Width="Auto" Height="225">
                                                <data:DataGrid.Columns>
                                                    <data:DataGridTextColumn Width="80" Binding="{Binding EmbSerialNumber,Mode=TwoWay}" Header="EmbSerialNumber" IsReadOnly="True" Visibility="Collapsed"/>
