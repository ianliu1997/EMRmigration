<UserControl x:Class="PalashDynamics.MIS.SubReportLinks"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
             xmlns:controlsToolkit="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Toolkit"
    >
    <controlsToolkit:DockPanel LastChildFill="True" >
        <controlsToolkit:DockPanel LastChildFill="True" x:Name="dpMasterList" Visibility="Collapsed">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
            </Grid>
        </controlsToolkit:DockPanel>

        <controlsToolkit:DockPanel LastChildFill="True" x:Name="dpCRM" Visibility="Collapsed">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition ></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" ></RowDefinition>
                    <RowDefinition Height="Auto" ></RowDefinition>
                    <RowDefinition Height="Auto" ></RowDefinition>
                    <RowDefinition Height="Auto" ></RowDefinition>
                    <RowDefinition Height="Auto" ></RowDefinition>
                    <RowDefinition Height="Auto" ></RowDefinition>
                    <RowDefinition Height="Auto" ></RowDefinition>
                    <RowDefinition Height="Auto" ></RowDefinition>
                    <RowDefinition Height="Auto" ></RowDefinition>
                    <RowDefinition Height="Auto" ></RowDefinition>
                </Grid.RowDefinitions>
                <HyperlinkButton Grid.Row="0" Grid.Column="0" Content="Customer/Card wise report" Margin="2" Click="HyperlinkButton_Click"  />
                <HyperlinkButton Grid.Row="1" Grid.Column="0" Content="Customer/Clinic wise revenue" Margin="2" Click="HyperlinkButton_Click"  />
                <HyperlinkButton Grid.Row="2" Grid.Column="0" Content="Card/non card patients revenue" Margin="2" Click="HyperlinkButton_Click"  />
                <HyperlinkButton Grid.Row="3" Grid.Column="0" Content="Area wise revenue /footfalls" Margin="2" Click="HyperlinkButton_Click"  />
                <HyperlinkButton Grid.Row="4" Grid.Column="0" Content="Card wise other Razi clinics visit report" Margin="2" Click="HyperlinkButton_Click"  />
                <HyperlinkButton Grid.Row="5" Grid.Column="0" Content="Card type wise report" Margin="2" Click="HyperlinkButton_Click"  />
                <HyperlinkButton Grid.Row="6" Grid.Column="0" Content="Visit frequency report" Margin="2" Click="HyperlinkButton_Click"  />
                <HyperlinkButton Grid.Row="7" Grid.Column="0" Content="Demographic reports" Margin="2" Click="HyperlinkButton_Click"  />
                <HyperlinkButton Grid.Row="8" Grid.Column="0" Content="Daily sales reports" Margin="2" Click="HyperlinkButton_Click"  />
                <HyperlinkButton Grid.Row="9" Grid.Column="0" Content="Monthly sales report" Margin="2" Click="HyperlinkButton_Click"  />
            </Grid>
        </controlsToolkit:DockPanel>
        <controlsToolkit:DockPanel LastChildFill="True" x:Name="dpSecurity" Visibility="Collapsed">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
            </Grid>
        </controlsToolkit:DockPanel>
        <controlsToolkit:DockPanel LastChildFill="True" x:Name="dpInventoryPharmacy" Visibility="Collapsed">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
            </Grid>
        </controlsToolkit:DockPanel>

        <controlsToolkit:DockPanel LastChildFill="True" x:Name="dpAccounts" Visibility="Collapsed">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
            </Grid>
        </controlsToolkit:DockPanel>

        <controlsToolkit:DockPanel LastChildFill="True" x:Name="dpClinical" Visibility="Collapsed">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
            </Grid>
        </controlsToolkit:DockPanel>

        <controlsToolkit:DockPanel LastChildFill="True" x:Name="dpCustomReport" Visibility="Collapsed">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
            </Grid>
        </controlsToolkit:DockPanel>
        
    </controlsToolkit:DockPanel>
    
    
</UserControl>
