<UserControl x:Class="PalashDynamics.MIS.IVF.IVFReport"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
              xmlns:controlsToolkit="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Toolkit" Loaded="UserControl_Loaded">
    <Border HorizontalAlignment="Stretch" BorderThickness="2" Margin="8,8,8,8" CornerRadius="0,0,3,3" Background="Transparent">
        <Grid x:Name="LayoutRoot" Background="White">
            <controlsToolkit:DockPanel LastChildFill="True" >

                <controlsToolkit:DockPanel LastChildFill="True" x:Name="Links" >
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition ></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                        </Grid.RowDefinitions>
                        <HyperlinkButton Grid.Row="0" Grid.Column="0" Content="Pregnancy Success Rate (Donor) " Margin="2" Click="HyperlinkButton_Click"  />
                        <HyperlinkButton Grid.Row="1" Grid.Column="0" Content="Pregnancy Success Rate (Self)" Margin="2" Click="HyperlinkButton2_Click"   />
                        <HyperlinkButton Grid.Row="2" Grid.Column="0" Content="Pregnancy Rate (Month Wise)" Margin="2" Click="HyperlinkButton3_Click"     />
                        <HyperlinkButton Grid.Row="3" Grid.Column="0" Content="Patient ET Count" Margin="2" Click="HyperlinkButton4_Click"/>
                        <HyperlinkButton Grid.Row="4" Grid.Column="0" Content="Patient OPU Count" Margin="2" Click="HyperlinkButton5_Click"  />
                        <HyperlinkButton Grid.Row="5" Grid.Column="0" Content="Patient Patient IVF ANC Summary" Margin="2" Click="HyperlinkButton_Click_1"  Visibility="Collapsed" />
                        <HyperlinkButton Grid.Row="6" Grid.Column="0" Content="Patient Investigation Report" Margin="2" Click="HyperlinkButton_Click_3"  Visibility="Collapsed" />
                        <!--By BHUSHAN-->
                        <HyperlinkButton Grid.Row="5" Grid.Column="0" Content="Implantation Rate" Margin="2" Click="ImplantationRate_Click"   />
                        <HyperlinkButton Grid.Row="6" Grid.Column="0" Content="Fertilization Rate" Margin="2" Click="FertilizationRate_Click"   />
                        <HyperlinkButton Grid.Row="7" Grid.Column="0" Content="On Going Pregnancy Rate" Margin="2" Click="OnGoingPregnancyRate_Click"  />
                        <HyperlinkButton Grid.Row="8" Grid.Column="0" Content="Pregnancy Rate" Margin="2" Click="BiochemicalPregnancyRate_Click"   />
                        <!--Biochemical Pregnancy Rate-->
                        <HyperlinkButton Grid.Row="9" Grid.Column="0" Content="Clinical Pregnancy Rate" Margin="2" Click="ChemicalPregnancyRate_Click"  />
                        <HyperlinkButton Grid.Row="10" Grid.Column="0" Content="Cumulative Pregnancy Rate" Margin="2" Click="CumulativePregnancyRate_Click"   />
                        <HyperlinkButton Grid.Row="11" Grid.Column="0" Content="Live Birth/Embryos Transferred" Margin="2" Click="LiveBirthRate_Click"   />
                        <HyperlinkButton Grid.Row="12" Grid.Column="0" Content="Cleavage Rate" Margin="2" Click="CleavageRate_Click"    />
                        <HyperlinkButton Grid.Row="13" Grid.Column="0" Content="Oocytes - Embryo Count" Margin="2" Click="occembryocount_Click"   />
                        <HyperlinkButton Grid.Row="14" Grid.Column="0" Content="Donor /Mother Report" Margin="2" Click="donormather_Click" Visibility="Collapsed"   />
                        <!--devidas-->
                        <HyperlinkButton Grid.Row="15" Content="IUI Patient Therapy Count" Margin="2" Click="IVIPatient_Click" />
                        <HyperlinkButton Grid.Row="16" Content="IVF AND IUI Patient Therapy Count" Margin="2" Click="IVIPatientTheropy_Click" Height="16" VerticalAlignment="Top" />
                        <HyperlinkButton Grid.Row="17" Content="OPU AND ET Patient Count" Margin="2" Click="OPUANDETPatient_Click"  />
                        <HyperlinkButton Grid.Row="18"  Grid.Column="0" Content="PGD/PGS Report" Margin="2" Click="HyperlinkButton_Click_2"  /> <!--margin = 2,0,-17,2-->
                        <HyperlinkButton Grid.Row="19"  Grid.Column="0" Content="IVF Patient Demographic" Margin="2" Click="HyperlinkButton_Click_4"  />
                        <HyperlinkButton Grid.Row="20"  Grid.Column="0" Content="Good Grade Embryos" Margin="2" Click="HyperlinkButton_Click_5"  />
                        <HyperlinkButton Grid.Row="21"  Grid.Column="0" Content="Miscarriage Rate" Margin="2" Click="HyperlinkButton_Click_6"  />

                    </Grid>

                </controlsToolkit:DockPanel>
                <controlsToolkit:DockPanel Visibility="Collapsed" x:Name="content_control">
                    <ContentControl x:Name="content"  ></ContentControl>
                </controlsToolkit:DockPanel>

            </controlsToolkit:DockPanel>
        </Grid>
    </Border>
</UserControl>
