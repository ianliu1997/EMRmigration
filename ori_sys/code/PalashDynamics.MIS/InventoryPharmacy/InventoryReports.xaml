<UserControl xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"  x:Class="PalashDynamics.MIS.InventoryPharmacy.InventoryReports"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    >

    <Border HorizontalAlignment="Stretch" BorderThickness="2" Margin="8,8,8,8" CornerRadius="0,0,3,3" Background="Transparent">
        <toolkit:DockPanel LastChildFill="True" >

            <toolkit:DockPanel  x:Name="Links" toolkit:DockPanel.Dock="Top">

                <Grid toolkit:DockPanel.Dock="Top">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition ></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                    </Grid.RowDefinitions>
                    <HyperlinkButton Grid.Row="0" Grid.Column="0" Content="ITEM WISE SALES REPORT" Margin="2"  x:Name="hlIteWiseSales" Click="hlItemwiseDailySalesReport_Click" Visibility="Visible" />
                    <HyperlinkButton Grid.Row="1" Visibility="Collapsed"  Grid.Column="0" Content="DAILY PHARMACY SALES SUMMARY REPORT" x:Name="hlDialyPharmacySalesSummary" Click="hlDialyPharmacySalesSummary_Click"  Margin="2"    />
                    <HyperlinkButton Grid.Row="2" Visibility="Collapsed"  Grid.Column="0" Content="DAILY PHARMACY SALES DETAILS REPORT" x:Name="hlDailyPharmacySalesDetail" Margin="2" Click="hlDailyPharmacySalesDetail_Click"  />
                    <HyperlinkButton Grid.Row="3" Visibility="Collapsed"  Grid.Column="0" Content="GRN AT CLINIC" x:Name="hlGRNatClinic" Margin="2" Click="hlGRNatClinic_Click" />
                    <HyperlinkButton Grid.Row="4" Visibility="Visible"  Grid.Column="0" Content="SALES RETURN" x:Name="hlSalesReturn" Margin="2" Click="hlSalesReturn_Click" />
                    <HyperlinkButton Grid.Row="5" Visibility="Collapsed"  Grid.Column="0" Content="STOCK STATEMENT" x:Name="hlStockStatement" Margin="2" Click="hlStockStatement_Click"  />
                    <HyperlinkButton Grid.Row="6" Visibility="Collapsed"  Grid.Column="0" Content="GRC" x:Name="hlGRC" Margin="2" Click="hlGRC_Click"  />
                    <HyperlinkButton Grid.Row="7" Visibility="Collapsed"  Grid.Column="0" Content="ITEM RETURN" x:Name="hlItemReturn" Margin="2" Click="hlItemReturn_Click"  />
                    <HyperlinkButton Grid.Row="8" Visibility="Visible"  Grid.Column="0" Content="INDENT PENDING FOR APPROVAL REPORT" x:Name="hlIndentPendingForApprovalReport" Margin="2" Click="hlIndentPendingForApprovalReport_Click" />
                    <HyperlinkButton Grid.Row="9" Visibility="Visible"  Grid.Column="0" Content="PURCHASE ORDER LIST" x:Name="hlPurchaseOrderList" Margin="2"  Click="hlPurchaseOrderList_Click" />
                    <HyperlinkButton Grid.Row="10" Visibility="Visible"  Grid.Column="0" Content="PURCHASE ORDER LIST ITEM WISE" x:Name="hlPurchaseOrderListItemwise" Margin="2"  Click="hlPurchaseOrderListItemWise_Click" />
                    <HyperlinkButton Grid.Row="11" Visibility="Visible"  Grid.Column="0" Content="GOODS RECEIVED NOTE REPORT ITEM WISE" x:Name="hlGoodsreceivedNoteReportItemWise" Margin="2"  Click="hlGoodsreceivedNoteReportItemWise_Click" />
                    <HyperlinkButton Grid.Row="12" Visibility="Visible"  Grid.Column="0" Content="GOODS RECEIVED NOTE REPORT" x:Name="hlGoodsreceivedNoteReport" Margin="2"  Click="hlGoodsreceivedNoteReport_Click" />
                    <HyperlinkButton Grid.Row="13" Visibility="Collapsed"  Grid.Column="0" Content="ISSUES TO CLINIC" x:Name="hlIssuestoClinic" Margin="2"  Click="hlIssuestoClinic_Click" />
                    <HyperlinkButton Grid.Row="14" Visibility="Visible"  Grid.Column="0" Content="RETURNS FROM CLINIC" x:Name="hlReturnsFromClinic" Margin="2"  Click="hlReturnsFromClinic_Click" />
                    <HyperlinkButton Grid.Row="15" Visibility="Collapsed"  Grid.Column="0" Content="STOCK LEDGER REPORT" x:Name="hlStockLedgerReport" Margin="2"  Click="hlStockLedgerReport_Click" />
                    <HyperlinkButton Grid.Row="16" Visibility="Collapsed" Grid.Column="0" Content="STOCK CHECK REPORT " x:Name="hlSotckCheckReport" Margin="2"  Click="hlSotckCheckReport_Click" />
                    <HyperlinkButton Grid.Row="17" Visibility="Visible"  Grid.Column="0" Content="STOCK REPORT WITH TRANSACTION DETAILED(EPR)" x:Name="hlAvailableStock" Margin="2"  Click="hlAvailableStock_Click" />
                    <HyperlinkButton Grid.Row="18" Visibility="Visible"  Grid.Column="0" Content="INDENT RECEIVE AND ISSUE REPORT" x:Name="hlIndentReceiveandIssueReport" Margin="2"  Click="hlIndentReceiveandIssueReport_Click" />
                    <HyperlinkButton Grid.Row="19" Visibility="Collapsed"  Grid.Column="0" Content="MATERIAL CONSUMPTION REPORT" x:Name="hlMaterialConsumptionReport" Margin="2" Click="hlMaterialConsumptionReport_Click" />
                    <HyperlinkButton Grid.Row="20" Visibility="Collapsed"  Grid.Column="0" Content="ABC ANALYSIS" x:Name="hlABCAnalysisReport" Margin="2" Click="hlABCAnalysisReport_Click" />
                    <HyperlinkButton Grid.Row="21" Visibility="Collapsed"  Grid.Column="0" Content="FNS ANALYSIS" x:Name="hlFNSAnalysisReport" Margin="2" Click="hlFNSAnalysisReport_Click"/>
                    <HyperlinkButton Grid.Row="22" Visibility="Visible"  Grid.Column="0" Content="ITEM LEDGER REPORT" x:Name="ItemLedgerReport" Margin="2" Click="ItemLedgerReport_Click"/>
                    <HyperlinkButton Grid.Row="23" Visibility="Collapsed"  Grid.Column="0" Content="ITEM LEDGER REPORT(Old)" x:Name="ItemLedgerReportOld" Margin="2" Click="ItemLedgerReportOld_Click"/>
                    <HyperlinkButton Grid.Row="24" Visibility="Visible"  Grid.Column="0" Content="QUOTATION COMPARISON REPORT" x:Name="QuotationCmpReport" Margin="2" Click="QuotationCMPReport_Click"/>
                    <HyperlinkButton Grid.Row="25" Visibility="Visible"  Grid.Column="0" Content="PHARMACY COLLECTION REPORT" x:Name="PharmacyCollectionReport" Margin="2" Click="PharmacyCollectionReport_Click"/>
                    <HyperlinkButton Grid.Row="26" Visibility="Visible"  Grid.Column="0" Content="INDENT TO GRN DETAILS REPORT" x:Name="IndentToGRNReport" Margin="2" Click="IndentToGRNReport_Click"/>
                    <HyperlinkButton Grid.Row="27" Visibility="Visible"  Grid.Column="0" Content="MATERIAL CONSUMPTIONS REPORT" x:Name="MaterialConsumptionsReport" Margin="2" Click="MaterialConsumptionsReport_Click"/>
                    <HyperlinkButton Grid.Row="28" Grid.Column="0" Content="ITEM WISE PACKAGE ISSUE" Margin="2"  x:Name="hlIteWisePackage" Click="hlItemwisePackageIssueReport_Click" Visibility="Visible" />
                    <HyperlinkButton Grid.Row="29" Grid.Column="0" Content="GRN LEDGER REPORT" Margin="2"  x:Name="hlGRNLEDGERREPORT" Click="hlGRNLEDGERREPORT_Click" Visibility="Visible" />
                    <HyperlinkButton Grid.Row="30" Grid.Column="0" Content="GRN LEDGER DETAILS REPORT" Margin="2"  x:Name="hlGRNLEDGERDETAILSREPORT" Click="hlGRNLEDGERDETAILSREPORT_Click" />
                    <HyperlinkButton Grid.Row="31" Visibility="Visible"  Grid.Column="0" Content="STOCK ADJUSTMENT REPORT" x:Name="StockAdjustmentReport" Margin="2" Click="StockAdjustmentReport_Click"/>
                    <HyperlinkButton Grid.Row="32" Visibility="Visible"  Grid.Column="0" Content="QUARANTINE ISSUE TO SCRAP/EXPIRY RETURN REPORT" x:Name="QuarantineIssueScrapExpirYReturNReport" Margin="2" Click="QuarantineIssueScrapExpirYReturNReport_Click"/>
                    <HyperlinkButton Grid.Row="33" Visibility="Visible"  Grid.Column="0" Content="QUARANTINE GRN ISSUE TO GRN RETURN REPORT" x:Name="GRNIssueQStoGRNReturnReport" Margin="2" Click="GRNIssueQStoGRNReturnReport_Click"/>




                </Grid >
            </toolkit:DockPanel>
            <toolkit:DockPanel Visibility="Collapsed" toolkit:DockPanel.Dock="Top" x:Name="content_control">
                <ContentControl x:Name="content"  ></ContentControl>
            </toolkit:DockPanel>


        </toolkit:DockPanel>


    </Border>

</UserControl>
