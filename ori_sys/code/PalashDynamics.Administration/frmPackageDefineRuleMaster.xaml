                                                                <ToggleButton.Content>
                                                                    <Path x:Name="BtnArrow" Height="4" Width="8" Stretch="Uniform" Data="F1 M 301.14,-189.041L 311.57,-189.041L 306.355,-182.942L 301.14,-189.041 Z " Margin="0,0,6,0" HorizontalAlignment="Right">
                                                                        <Path.Fill>
                                                                            <SolidColorBrush x:Name="BtnArrowColor" Color="#FF333333"/>
                                                                        </Path.Fill>
                                                                    </Path>
                                                                </ToggleButton.Content>
                                                            </ToggleButton>
                                                        </Grid>
                                                    </ControlTemplate>
                                                </Setter.Value>
                                            </Setter>
                                        </Style>
                                    </Grid.Resources>

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                                        <!--<ColumnDefinition Width="*"></ColumnDefinition>-->
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"></RowDefinition>
                                        <RowDefinition Height="Auto"></RowDefinition>
                                        <RowDefinition Height="Auto"></RowDefinition>
                                        <RowDefinition Height="Auto"></RowDefinition>
                                        <RowDefinition Height="Auto"></RowDefinition>
                                        <RowDefinition Height="Auto"></RowDefinition>
                                        <RowDefinition Height="Auto"></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <TextBlock Text="{Binding ItemTextValue}" Width="110" Grid.Column="2" Grid.Row="0" />                                    
                                    <CheckBox x:Name="chkAdjustableHead"  Content="Adjustable Head" TabIndex="0" Grid.Column="0" Grid.Row="0"  Margin="4"  HorizontalAlignment="Stretch"  HorizontalContentAlignment="Left" Checked="chkAdjustableHead_Checked" Unchecked="chkAdjustableHead_Unchecked" Visibility="Visible"></CheckBox>

                                    <StackPanel x:Name="stkAdjustableHead" Orientation="Horizontal" Grid.ColumnSpan="4" Margin="4" Grid.Row="0" Grid.Column="1" Visibility="Collapsed" >
                                        <RadioButton x:Name="rdbClinicalHead" Content="Clinical" Margin="4" IsChecked="True" ></RadioButton>
                                        <RadioButton x:Name="rdbPharmacyHead"  Content="Pharmacy" Margin="4" ></RadioButton>
                                    </StackPanel>
                                    
                                    <TextBlock HorizontalAlignment="Right" VerticalAlignment="Center"  Grid.Row="2"  Grid.Column="0"  Margin="4" Text="Procedure" />
