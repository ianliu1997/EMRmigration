                                    <CheckBox x:Name="chkIsConsumables"  Content="Is Consumables" TabIndex="0" Grid.Column="2" Grid.ColumnSpan="2" Grid.Row="2"  Margin="4"  HorizontalAlignment="Stretch"  HorizontalContentAlignment="Left"  Visibility="Visible"></CheckBox>

                                    <TextBlock HorizontalAlignment="Right" VerticalAlignment="Center"  Grid.Row="3"  Grid.Column="0"  Margin="4" Text="Gender" />
