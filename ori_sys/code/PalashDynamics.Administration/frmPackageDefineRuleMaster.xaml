                                    <CheckBox x:Name="chkAllRelations"  Content="Select All Relations"    TabIndex="0" Grid.Column="1" Grid.Row="4"  Margin="4"  HorizontalAlignment="Stretch" Checked="chkAllRelations_Checked" Unchecked="chkAllRelations_Checked" HorizontalContentAlignment="Left" Visibility="Visible"></CheckBox>

