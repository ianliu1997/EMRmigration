                                    <TextBlock HorizontalAlignment="Right" VerticalAlignment="Center"  Grid.Row="3"  Grid.Column="4"  Margin="4" Text="Discount (%)" />
                                    <TextBox x:Name="txtDiscount" TextAlignment="Right"  Text="{Binding Discount,Mode=TwoWay}" HorizontalAlignment="Stretch" VerticalAlignment="Center" Grid.Row="3" Grid.Column="5" Width="Auto" Margin="4" TextChanged="txtDiscount_TextChanged" KeyDown="txtDiscount_KeyDown" LostFocus="txtDiscount_LostFocus"></TextBox>

                                    <CheckBox x:Name="chkDiscountOnQuantity"  Content="Discount On Quantity"  IsChecked="{Binding IsDiscountOnQuantity,Mode=TwoWay}"  TabIndex="0" Grid.Column="6" Grid.Row="3"  Margin="4" HorizontalAlignment="Stretch" ></CheckBox>

                                    <TextBlock HorizontalAlignment="Right" VerticalAlignment="Center"  Grid.Row="3"  Grid.Column="2"  Margin="4" Text="Age Limit" />
                                    <TextBox x:Name="txtAgeLimit" TextAlignment="Right"  Text="{Binding AgeLimit,Mode=TwoWay}" HorizontalAlignment="Stretch" VerticalAlignment="Center" Grid.Row="3" Grid.Column="3" Width="Auto" MaxLength="3" Margin="4" TextChanged="txtAgeLimit_TextChanged" KeyDown="txtAgeLimit_KeyDown"></TextBox>

                                    <TextBlock HorizontalAlignment="Right" VerticalAlignment="Center"  Grid.Row="4"  Grid.Column="4"  Margin="4" Text="Quantity" />
