<UserControl x:Class="PalashDynamics.Administration.frmPathologyParameterMaster"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk" 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"    
    xmlns:controlsToolkit="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Toolkit"               
    xmlns:controls="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls"
    xmlns:input="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Input"
    xmlns:inputcombo="clr-namespace:PalashDynamics.Controls;assembly=PalashDynamics.Controls.AutoCompleteComboBox"     
    xmlns:data="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Data" 
    xmlns:dataInput="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Data.Input"
    xmlns:Grp="clr-namespace:Groupbox;assembly=Groupbox" 
    Height="Auto" Width="Auto" Loaded="UserControl_Loaded">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/PalashDynamics;component/Generic.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid x:Name="LayoutRoot"  Margin="2" Background="White" Width="Auto">
        <Grid>
            <controlsToolkit:DockPanel x:Name="docPathoParameterMaster" LastChildFill="True" controlsToolkit:DockPanel.Dock="Top">
                <Border BorderBrush="{StaticResource TabControlDarkerBorder}" Margin="4" controlsToolkit:DockPanel.Dock="Bottom">
                    <StackPanel x:Name="Footer" Orientation="Horizontal" VerticalAlignment="Center" Margin="0,3,5,3" HorizontalAlignment="Right" Background="Transparent">
                        <StackPanel Orientation="Horizontal"  Margin="1" HorizontalAlignment="Right">
                            <ToggleButton x:Name="cmdNew" Content="New" Click="cmdNew_Click" Background="#000000" Margin="5,0,0,0" HorizontalAlignment="Stretch" Style="{StaticResource ToggleButtonStyle}" Visibility="Visible"></ToggleButton>
                            <ToggleButton x:Name="cmdSave" Content="Save" Background="#000000" Margin="5,0,0,0" HorizontalAlignment="Stretch" Style="{StaticResource ToggleButtonStyle}" Click="cmdSave_Click" TabIndex="20"></ToggleButton>
                            <ToggleButton Content="Modify" x:Name="cmdModify" Background="#000000" Margin="5,0,0,0" HorizontalAlignment="Stretch" Style="{StaticResource ToggleButtonStyle}" Click="cmdModify_Click" TabIndex="21"></ToggleButton>
                            <ToggleButton Content="Cancel" x:Name="cmdCancel" Background="#000000" Margin="5,0,0,0" HorizontalAlignment="Stretch" Style="{StaticResource ToggleButtonStyle}" Click="cmdCancel_Click" TabIndex="22"></ToggleButton>
                        </StackPanel>
                    </StackPanel>
                </Border>

                <Grid x:Name="PageContainer">
                    <Grid x:Name="LayoutRoot1" Visibility="Visible" Background="White" Grid.Row="0">
                        <controlsToolkit:DockPanel Margin="4" LastChildFill="True">
                            <Border  controlsToolkit:DockPanel.Dock="Top" BorderBrush="{StaticResource TabControlDarkerBorder }" BorderThickness="1" CornerRadius="3,3,0,0" Background="Transparent">
                                <ContentControl VerticalAlignment="Center" Grid.ColumnSpan="4" FontSize="12" Content="List of Pathology Parameters Details" Margin="-1" Style="{StaticResource Header}" HorizontalAlignment="Stretch"/>
                            </Border>

                            <Border BorderBrush="{StaticResource TabControlDarkerBorder}" Margin="0,-1,0,0" BorderThickness="1" CornerRadius="0,0,3,3" Background="Transparent">
                                <controlsToolkit:DockPanel Margin="4" LastChildFill="True">
                                    <Grid controlsToolkit:DockPanel.Dock="Top">
                                        <Grid.RowDefinitions>
                                            <RowDefinition></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <!--<ColumnDefinition Width="*"/>-->
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>

                                        <StackPanel Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="4" Orientation="Horizontal">
                                            <TextBlock x:Name="txtSearch" Height="24" Text="Description" Margin="4,10,4,4" Width="75"/>
                                            <TextBox x:Name="txtSearchCriteria" Height="24" Text="" Margin="0,0,4,4" Width="300"  MaxLength="150" KeyUp="txtSearchCriteria_KeyUp"/>
                                            <Button x:Name="cmdSearch" Click="cmdSearch_Click" Margin="4"  ToolTipService.ToolTip="Search Code/Description" >
                                                <Image Source="/PalashDynamics;component/Icons/xmag.png" Margin="2" Width="20" ></Image>
                                            </Button>
                                        </StackPanel>
                                    </Grid>

                                    <data:DataPager x:Name="dgDataPager" controlsToolkit:DockPanel.Dock="Bottom" 
                                                    DisplayMode="FirstLastPreviousNext" Source="{Binding}" IsEnabled="True"
                                                    AutoEllipsis="False" IsTotalItemCountFixed="False"/>

                                    <data:DataGrid  x:Name="dgPathoParameterList" Margin="0,0,0,1" ItemsSource="{Binding}" AutoGenerateColumns="False" SelectionChanged="dgPathoParameterList_SelectionChanged" CellEditEnding="dgPathoParameterList_CellEditEnding" CellEditEnded="dgPathoParameterList_CellEditEnded">
                                        <data:DataGrid.Columns>
                                            <data:DataGridTemplateColumn>
                                                <data:DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <HyperlinkButton VerticalAlignment="Center"  Content="View"  Padding="5,0,5,0" Click="Edit_Click"  x:Name="hlbEditTemplate"></HyperlinkButton>
                                                    </DataTemplate>

                                                </data:DataGridTemplateColumn.CellTemplate>
                                            </data:DataGridTemplateColumn>
                                            <data:DataGridTextColumn Binding="{Binding Code, Mode=TwoWay}" Header=" Parameter Code" IsReadOnly="True"/>
                                            <data:DataGridTextColumn Binding="{Binding ParameterDesc,Mode=TwoWay}" Header="Parameter Name" IsReadOnly="True"/>
                                            <data:DataGridTextColumn Binding="{Binding PrintName,Mode=TwoWay}" Header="Print Name" IsReadOnly="True"/>
                                            <data:DataGridTextColumn Binding="{Binding stringstrParameterUnitName,Mode=TwoWay}" Header="Unit" IsReadOnly="True"/>
                                            <data:DataGridTemplateColumn Header="Status" IsReadOnly="True">
                                                <data:DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <CheckBox Margin="4"  x:Name="chkStatus"  Click="chkStatus_Click" IsChecked="{Binding Status,Mode=TwoWay}" VerticalAlignment="Center" HorizontalAlignment="Center"></CheckBox >
                                                    </DataTemplate>
                                                </data:DataGridTemplateColumn.CellTemplate>
                                            </data:DataGridTemplateColumn>
                                        </data:DataGrid.Columns>
                                    </data:DataGrid>

                                </controlsToolkit:DockPanel>
                            </Border>
                        </controlsToolkit:DockPanel>
                    </Grid>

                    <!-- PathologyParameter Details -->

                    <Grid x:Name="LayoutRoot2" Visibility="Collapsed" Background="White" Grid.Row="1">
                        <controlsToolkit:DockPanel Margin="4" LastChildFill="True">
                            <controlsToolkit:DockPanel x:Name="docHeader" controlsToolkit:DockPanel.Dock="Top">
                                <Grp:GroupBox x:Name="grpParameterDetails" Header="Parameter Details" IsTabStop="False" Height="Auto" Grid.Column="0" Grid.Row="0" Margin="4">
                                    <!--<Border BorderBrush="{StaticResource TabControlDarkerBorder}" BorderThickness="1" Margin="3"  CornerRadius="0,0,0,0" Background="Transparent" Height="auto" Width="auto">-->
                                    <Grid Margin="4" HorizontalAlignment="Stretch">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto" />
                                                <!--<RowDefinition Height="Auto" />
                                                <RowDefinition Height="Auto" />
                                                <RowDefinition Height="Auto" />
                                                <RowDefinition Height="Auto" />-->
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock x:Name="ParameterCode" Text="Parameter Code" HorizontalAlignment="Right"  VerticalAlignment="Center" Margin="4" Grid.Row="0" Grid.Column="0"/>
                                        <TextBox x:Name="txtCode" HorizontalAlignment="Stretch" MaxLength="10" Text ="{Binding Code, Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="0" Grid.Column="1"  Margin="4" AcceptsReturn="True" TabIndex="0" ></TextBox>

                                        <TextBlock x:Name="ParameterName" Text="Parameter Name" HorizontalAlignment="Right"  VerticalAlignment="Center" Margin="4" Grid.Row="0" Grid.Column="2"/>
                                        <TextBox x:Name="txtName" MaxLength="100" Text="{Binding ParameterDesc, Mode=TwoWay}"  HorizontalAlignment="Stretch"   VerticalAlignment="Center" Grid.Row="0" Grid.Column="3"  Margin="4" AcceptsReturn="True" TabIndex="1"></TextBox>

                                        <TextBlock x:Name="PrintName" Text="Print Name" HorizontalAlignment="Right"  VerticalAlignment="Center" Margin="4" Grid.Row="1"/>
                                        <TextBox x:Name="txtPrintName" Text ="{Binding PrintName,Mode=TwoWay}" HorizontalAlignment="Stretch" VerticalAlignment="Center"  Grid.Row="1" Grid.Column="1"  Margin="4" MaxLength="500" AcceptsReturn="True" TabIndex="2"></TextBox>
                                        
                                        <!-- Added on 22/2/2016 Technique Used-->
                                        <TextBlock  Grid.Row="3" Grid.Column="0" Text="Technique Used" Margin="4" VerticalAlignment="Center" HorizontalAlignment="Right"></TextBlock>
                                        <TextBox  Grid.Row="3" Grid.Column="1" x:Name="txtTechUsed" Text="{Binding Technique,Mode=TwoWay}"  Margin="4"  LostFocus="txtStoreCode_LostFocus" MaxLength="100" HorizontalAlignment="Stretch" Grid.ColumnSpan="2" TabIndex="12"></TextBox>                                    




                                        <!--added by rohini dated 14/1/2016-->
                                        <TextBlock x:Name="Delta" Text="Delta Check Percent (%)" HorizontalAlignment="Right"  VerticalAlignment="Center" Margin="4" Grid.Row="2" Grid.Column="2"/>
                                        <TextBox x:Name="txtDelta" Width="50" HorizontalAlignment="Left" TextAlignment="Right" Text ="{Binding DeltaCheckPer,Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="2" Grid.Column="3"  Margin="4" MaxLength="2" AcceptsReturn="True" TabIndex="4"  KeyDown="txtMinValue_KeyDown"  ></TextBox>


                                        <!--<TextBlock Text="%" HorizontalAlignment="Right"  VerticalAlignment="Center" Margin="4" Grid.Row="2" Grid.Column="2"/>-->
                                        <TextBlock x:Name="TypeOfValue" Text="Type Of Value" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="4" Grid.Row="1" Grid.Column="2"/>
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Row="1" Grid.Column="3">
                                            <RadioButton x:Name="optNumeric" Content="Numeric" IsChecked="True" Checked="optNumeric_Checked"/>
                                            <RadioButton x:Name="optValue" Content="Text" Checked="optValue_Checked"/>
                                        </StackPanel>

                                        <TextBlock x:Name="Unit" Text="Parameter Unit" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="4" Grid.Row="2" Grid.Column="0"/>
                                        <inputcombo:AutoCompleteComboBox x:Name="cmbUnit"  ItemsSource="{Binding}" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID"  Grid.Row="2" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="4" TabIndex="3" Width="193"></inputcombo:AutoCompleteComboBox>
                                        <!--<ComboBox  Grid.Row="2" Grid.Column="1" x:Name="cmbUnit" Margin="4" SelectedValue="ID" SelectedValuePath="ID" TabIndex="3"></ComboBox>-->

                                        <!--CheckBox x:Name="chkSpace" Content="Has Space" Grid.Row="2" Margin="4,8,0,0" HorizontalAlignment="Left" IsChecked="{Binding Path=HasSpace,Mode=TwoWay}" Grid.Column="2"/>-->



                                    </Grid>
                                    <!--</Border>-->

                                </Grp:GroupBox>

                            </controlsToolkit:DockPanel>

                            <controlsToolkit:DockPanel LastChildFill="True" x:Name="docPathoTab">
                                <Grid>
                                    <controls:TabControl Padding="0"  BorderBrush="{StaticResource TabControlDarkerBorder}"  x:Name="PathoDetailsInfo" Margin="4,-4,4,4" SelectionChanged="PathoDetailsInfo_SelectionChanged">
                                        <controls:TabItem x:Name="TabDefaultInfo" Style="{StaticResource TabItemStyle}" Padding="0" Margin="0" Header="Defaults">
                                            <controlsToolkit:DockPanel x:Name="DefaultDocMain1" controlsToolkit:DockPanel.Dock="Top" LastChildFill="True">
                                                <controlsToolkit:DockPanel x:Name="DefaultdocContet2" controlsToolkit:DockPanel.Dock="Top" LastChildFill="True">

                                                    <controlsToolkit:DockPanel x:Name="DefaultdocContet3" controlsToolkit:DockPanel.Dock="Bottom" >
                                                        <!--<ScrollBar Visibility="Visible">-->
                                                        <ScrollViewer ScrollViewer.HorizontalScrollBarVisibility="Visible" ScrollViewer.VerticalScrollBarVisibility="Visible"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                                                            <Grid>
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                                </Grid.ColumnDefinitions>
                                                                <Grid.RowDefinitions>
                                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                                </Grid.RowDefinitions>

                                                                <TextBlock x:Name="Category" Text="Category" HorizontalAlignment="Right"  VerticalAlignment="Center" Margin="4" Grid.Row="0" Grid.Column="0" />
                                                                <!--<ComboBox  Grid.Row="0" Grid.Column="1" x:Name="cmbCategory" Margin="4" SelectedValue="CategoryID" SelectedValuePath="CategoryID"></ComboBox>-->
                                                                <inputcombo:AutoCompleteComboBox x:Name="cmbCategory" Width="100" ItemsSource="{Binding}" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID" Grid.Row="0" Grid.Column="1"  HorizontalAlignment="Left" VerticalAlignment="Center" Margin="4" TabIndex="6"></inputcombo:AutoCompleteComboBox>

                                                                <TextBlock x:Name="Machine" Text="Machine Name" HorizontalAlignment="Right"  VerticalAlignment="Center" Margin="4" Grid.Row="0" Grid.Column="2" />
                                                                <!--<ComboBox  Grid.Row="0" Grid.Column="1" x:Name="cmbCategory" Margin="4" SelectedValue="CategoryID" SelectedValuePath="CategoryID"></ComboBox>-->
                                                                <inputcombo:AutoCompleteComboBox x:Name="cmbMachine" Width="100" ItemsSource="{Binding}" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID" Grid.Row="0" Grid.Column="3"   HorizontalAlignment="Left" VerticalAlignment="Center" Margin="4" TabIndex="7"></inputcombo:AutoCompleteComboBox>


                                                                <CheckBox x:Name="chkAgeApplicable" Content="Is Age Applicable" Grid.Row="1" Grid.Column="0" Margin="4,8,0,0" HorizontalAlignment="Left" IsChecked="{Binding Path=IsAgeApplicable,Mode=TwoWay}"  Checked="chkAgeApplicable_Checked" Unchecked="chkAgeApplicable_Checked" TabIndex="12"/>

                                                                <TextBlock x:Name="AgeFrom"  Text="Age From" HorizontalAlignment="Right"  VerticalAlignment="Center" Margin="4"  Grid.Row="1" Grid.Column="2" />
                                                                <TextBox x:Name="txtAgeFrom" TextAlignment="Right" IsEnabled="False" Text ="{Binding DefaultValues.AgeFrom, Mode=TwoWay}" Width="50" HorizontalAlignment="Left" VerticalAlignment="Center" TabIndex="14" Grid.Row="1" Grid.Column="3" Margin="4" TextChanged="txtAgeFrom_TextChanged_1" KeyDown="txtAgeFrom_KeyDown" MaxLength="3" ></TextBox>

                                                                <TextBlock x:Name="AgeTo" Text="Age To" HorizontalAlignment="Right"  VerticalAlignment="Center" Margin="4" Grid.Row="1" Grid.Column="4"/>
                                                                <TextBox x:Name="txtAgeTo" TextAlignment="Right" IsEnabled="False" Text ="{Binding DefaultValues.AgeTo, Mode=TwoWay}" Width="50" TabIndex="15" HorizontalAlignment="Left" VerticalAlignment="Center"  Grid.Row="1" Grid.Column="5" Margin="4" TextChanged="txtAgeTo_TextChanged" KeyDown="txtAgeTo_KeyDown" MaxLength="3" ></TextBox>
                                                                <inputcombo:AutoCompleteComboBox IsEnabled="False" x:Name="cmbAge" Width="70" ItemsSource="{Binding}" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID" Grid.Row="1" Grid.Column="1"   HorizontalAlignment="Left" VerticalAlignment="Center" Margin="4" TabIndex="13"></inputcombo:AutoCompleteComboBox>
                                                                
                                                                <TextBlock x:Name="MinVaule" Text="Min Reference" HorizontalAlignment="Right"  VerticalAlignment="Center" Margin="4" Grid.Row="0" Grid.Column="4"/>
                                                                <TextBox x:Name="txtMinValue" TextAlignment="Right" HorizontalAlignment="Left" Width="50" Text ="{Binding DefaultValues.MinValue, Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="0" Grid.Column="5" Margin="4" TabIndex="8" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" GotFocus="txtCostPrice_GotFocus" MaxLength="7" ></TextBox>

                                                                <TextBlock x:Name="MaxValue" Text="Max Reference" HorizontalAlignment="Right"  VerticalAlignment="Center" Margin="4" Grid.Row="0" Grid.Column="6"/>
                                                                <TextBox x:Name="txtMaxValue" TextAlignment="Right" HorizontalAlignment="Left" Width="50" VerticalAlignment="Center" Text ="{Binding DefaultValues.MaxValue, Mode=TwoWay}"  Grid.Row="0" Grid.Column="7" Margin="4" TabIndex="9" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown"  MaxLength="7" ></TextBox>

                                                                <!--commneted by rohini dated 12.4.16-->
                                                                <TextBlock Visibility="Collapsed" x:Name="DefaultValue" Text="Default Value" HorizontalAlignment="Right"  VerticalAlignment="Center" Margin="4" Grid.Row="0" Grid.Column="8"/>
                                                                <TextBox   Visibility="Collapsed" x:Name="txtDefaultValue" TextAlignment="Right" HorizontalAlignment="Left" Width="50" Text ="{Binding DefaultValues.DefaultValue, Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="0" Grid.Column="9" Margin="4" TabIndex="10" LostFocus="txtDefaultValue_LostFocus" TextChanged="txtAgeFrom_TextChanged" KeyDown="txtAgeFrom_KeyDown" MaxLength="7"></TextBox>

                                                                <!--added by rohini dated 14/1/2016-->
                                                                <TextBlock x:Name="MinImprobable" Text="Improbable Value ≤" HorizontalAlignment="Right"  VerticalAlignment="Center" Margin="4" Grid.Row="1" Grid.Column="6"/>
                                                                <TextBox x:Name="txtMinImprobable" TextAlignment="Right" HorizontalAlignment="Left" Width="50" Text ="{Binding DefaultValues.MinImprobable, Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="1" Grid.Column="7" Margin="4" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" TabIndex="16" MaxLength="7"></TextBox>
                                                                <!--added by rohini dated 14/1/2016-->
                                                                <TextBlock x:Name="MaxImprobable" Text="Improbable Value ≥" HorizontalAlignment="Right"  VerticalAlignment="Center" Margin="4" Grid.Row="1" Grid.Column="8"/>
                                                                <TextBox x:Name="txtMaxImprobable" TextAlignment="Right" HorizontalAlignment="Left" Width="50" VerticalAlignment="Center" Text ="{Binding DefaultValues.MaxImprobable, Mode=TwoWay}"  Grid.Row="1" Grid.Column="9" Margin="4" TabIndex="17" LostFocus="txtMaxValue_LostFocus" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="7" ></TextBox>
                                                                <CheckBox x:Name="chkIsFlagReflex" Content="Is Reflex Testing"  Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" Margin="4,8,0,0" HorizontalAlignment="Left" IsChecked="{Binding Path=IsFlagReflex,Mode=TwoWay}"  Checked="chkIsFlagReflex_Checked" Unchecked="chkIsFlagReflex_Checked" TabIndex="18"/>

                                                                 <!-- Varying Reference Ranges-->
                                                                <TextBlock x:Name="VaryingReferences" Text="Reference Ranges" HorizontalAlignment="Right"  VerticalAlignment="Center" Margin="4" Grid.Row="3" Grid.Column="0"/>
                                                                <TextBox x:Name="txtVaryingReferences" HorizontalScrollBarVisibility="Visible" VerticalScrollBarVisibility="Visible" HorizontalAlignment="Left"  Text ="{Binding DefaultValues.VaryingReferences, Mode=TwoWay}" AcceptsReturn="True" VerticalAlignment="Center"  Grid.Row="3" Grid.Column="1" Margin="4" TabIndex="23" LostFocus="txtDefaultValue_LostFocus" Grid.ColumnSpan="5" Width="510"></TextBox>


                                                                <!--added by rohini dated 14/1/2016-->
                                                                <TextBlock x:Name="Note" Text="Note" HorizontalAlignment="Right"  VerticalAlignment="Center" Margin="4" Grid.Row="4" Grid.Column="0"/>
                                                                <TextBox x:Name="txtNote" HorizontalScrollBarVisibility="Visible" VerticalScrollBarVisibility="Visible" HorizontalAlignment="Left"  Text ="{Binding Note.Note, Mode=TwoWay}" AcceptsReturn="True" VerticalAlignment="Center"  Grid.Row="4" Grid.Column="1" Margin="4" TabIndex="23" LostFocus="txtDefaultValue_LostFocus" Grid.ColumnSpan="5" Width="510"></TextBox>

                                                                <!--added by rohini dated 22/4/2016-->
                                                                <TextBlock x:Name="LowReffValue" Text="Min Authorization  " HorizontalAlignment="Right"  VerticalAlignment="Center" Margin="4" Grid.Row="0" Grid.Column="8"/>
                                                                <TextBox x:Name="txtLowReffValue" TextAlignment="Right" HorizontalAlignment="Left" Width="50" Text ="{Binding DefaultValues.LowReffValue, Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="0" Grid.Column="9" Margin="4" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" TabIndex="10" MaxLength="7"></TextBox>
                                                                <!--added by rohini dated 22/4/2016-->
                                                                <TextBlock x:Name="HighReffValue" Text="Max Authorization" HorizontalAlignment="Right"  VerticalAlignment="Center" Margin="4" Grid.Row="0" Grid.Column="10"/>
                                                                <TextBox x:Name="txtHighReffValue" TextAlignment="Right" HorizontalAlignment="Left" Width="50" VerticalAlignment="Center" Text ="{Binding DefaultValues.HighReffValue, Mode=TwoWay}"  Grid.Row="0" Grid.Column="11" Margin="4" TabIndex="14" LostFocus="txtMaxValue_LostFocus" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="7" ></TextBox>

                                                                <!--added by rohini dated 22/4/2016-->
                                                                <TextBlock x:Name="LowerPanicValue" Text="Panic Value ≤" HorizontalAlignment="Right"  VerticalAlignment="Center" Margin="4" Grid.Row="2" Grid.Column="6"/>
                                                                <TextBox x:Name="txtLowerPanicValue" TextAlignment="Right" HorizontalAlignment="Left" Width="50" VerticalAlignment="Center" Text ="{Binding DefaultValues.LowerPanicValue, Mode=TwoWay}"  Grid.Row="2" Grid.Column="7" Margin="4" TabIndex="21" LostFocus="txtMaxValue_LostFocus" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="7" ></TextBox>

                                                                <!--added by rohini dated 22/4/2016-->
                                                                <TextBlock x:Name="UpperPanicValue" Text="Panic  Value ≥" HorizontalAlignment="Right"  VerticalAlignment="Center" Margin="4" Grid.Row="2" Grid.Column="8"/>
                                                                <TextBox x:Name="txtUpperPanicValue" TextAlignment="Right" HorizontalAlignment="Left" Width="50" Text ="{Binding DefaultValues.UpperPanicValue, Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="2" Grid.Column="9" Margin="4" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" TabIndex="22" MaxLength="7"></TextBox>


                                                                <TextBlock x:Name="LowerReflexValue" Text="Reflex Value ≤" HorizontalAlignment="Right"  VerticalAlignment="Center" Margin="4" Grid.Row="2" Grid.Column="2"/>
                                                                <TextBox x:Name="txtLowerReflexValue" IsEnabled="False" TextAlignment="Right" HorizontalAlignment="Left" Width="50" VerticalAlignment="Center" Text ="{Binding DefaultValues.LowReflexValue, Mode=TwoWay}"  Grid.Row="2" Grid.Column="3" Margin="4" TabIndex="19" LostFocus="txtMaxValue_LostFocus" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" MaxLength="7" ></TextBox>

                                                                <!--added by rohini dated 22/4/2016-->
                                                                <TextBlock x:Name="HighReflexValue" Text="Reflex  Value ≥" HorizontalAlignment="Right"  VerticalAlignment="Center" Margin="4" Grid.Row="2" Grid.Column="4"/>
                                                                <TextBox x:Name="txtHighReflexValue" IsEnabled="False" TextAlignment="Right" HorizontalAlignment="Left" Width="50" Text ="{Binding DefaultValues.HighReflexValue, Mode=TwoWay}" VerticalAlignment="Center"  Grid.Row="2" Grid.Column="5" Margin="4" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" TabIndex="20" MaxLength="7"></TextBox>

                                                                <StackPanel x:Name="DefaultFooter1" Orientation="Horizontal" VerticalAlignment="Top" Margin="0,3,5,3" HorizontalAlignment="Right" Background="Transparent" Grid.Row="4" Grid.Column="6" Grid.ColumnSpan="2">
                                                                    <StackPanel Orientation="Horizontal"  Margin="1" HorizontalAlignment="Right"  >
                                                                        <ToggleButton x:Name="cmdAddToListDefault" Content="Add" Click="cmdAddToListDefault_Click" Background="#000000" ToolTipService.ToolTip="Add To List" Margin="5,0,0,0" Style="{StaticResource ToggleButtonStyle}" TabIndex="25"></ToggleButton>
                                                                        <ToggleButton Content="Modify" x:Name="cmdModifToListDefault" Background="#000000" Margin="5,0,0,0" HorizontalAlignment="Stretch" Style="{StaticResource ToggleButtonStyle}" TabIndex="26" Click="cmdModifyToListDefault_Click"></ToggleButton>
                                                                        <ToggleButton Visibility="Collapsed" x:Name="cmdAddService" Content="Add Service" Click="cmdAddService_Click" Background="#000000" ToolTipService.ToolTip="Add Services to default Parameter values" Margin="5,0,3,0" Style="{StaticResource ToggleButtonStyle}" TabIndex="19"  ></ToggleButton>
                                                                    </StackPanel>
                                                                </StackPanel>
                                                            </Grid>

                                                        </ScrollViewer>
                                                        <!--</ScrollBar>-->
                                                    </controlsToolkit:DockPanel>
                                                </controlsToolkit:DockPanel>

                                                <controlsToolkit:DockPanel x:Name="DefaultdocContet1" controlsToolkit:DockPanel.Dock="Top">
                                                    <data:DataGrid x:Name="dgDefaultValues" AutoGenerateColumns="False" ItemsSource="{Binding}" HorizontalScrollBarVisibility="Visible" VerticalScrollBarVisibility="Visible" IsReadOnly="True" Margin="4" HorizontalAlignment="Stretch"  TabIndex="12" SelectionChanged="dgDefaultValues_SelectionChanged" >
                                                        <data:DataGrid.Columns>
                                                            <data:DataGridTextColumn Binding="{Binding Category}"  Header="Category"></data:DataGridTextColumn>
                                                            <data:DataGridTextColumn Binding="{Binding Machine}"  Header="Machine"></data:DataGridTextColumn>
                                                            <data:DataGridTextColumn Binding="{Binding MinValue}" Header="Min Reference">
                                                                <data:DataGridTextColumn.ElementStyle>
