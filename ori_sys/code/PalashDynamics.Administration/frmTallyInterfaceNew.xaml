                                                    </Grid>

                                                </Grid>


                                            </Grid>

                                        </controlsToolkit:DockPanel>
                                    </controls:TabItem>
                                    <controls:TabItem x:Name="tbRefundOfAdvance" Style="{StaticResource TabItemStyle}"  Padding="0" Margin="0" Header="Refund Of Advance" Visibility="Visible" >
                                        <controlsToolkit:DockPanel LastChildFill="True" >

                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                </Grid.RowDefinitions>
                                                <data:DataGrid x:Name="dgRefundOfAdvance" VerticalAlignment="Stretch" Grid.Row="0" HorizontalAlignment="Stretch" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                                    <data:DataGrid.Columns>
                                                        <data:DataGridTextColumn Header="Ledger Name" Width=".20*"  Binding="{Binding LedgerName, Mode=OneWay}" IsReadOnly="True" />

                                                        <data:DataGridTextColumn  Width=".20*" IsReadOnly="True" Binding="{Binding DR,StringFormat=0.000, Mode=TwoWay}" Header="Dr" >
                                                            <data:DataGridTextColumn.ElementStyle>
                                                                <Style TargetType="TextBlock">
