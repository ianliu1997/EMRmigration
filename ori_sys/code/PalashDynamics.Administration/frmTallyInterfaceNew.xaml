                                                            <ColumnDefinition Width=".25*"></ColumnDefinition>
                                                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                            <ColumnDefinition Width=".25*"></ColumnDefinition>
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Grid.Column="0" Margin="4,8,4,4" Text="Total Debit"></TextBlock>
                                                        <TextBox Grid.Column="1" Margin="4"   IsEnabled="False" x:Name="txtAdvanceDr" TextAlignment="Right" HorizontalAlignment="Stretch" VerticalAlignment="Center" ></TextBox>
                                                        <TextBlock  Grid.Column="2" Margin="4,8,4,4" VerticalAlignment="Center" HorizontalAlignment="Right" Text="Total Credit"></TextBlock>

