                                                            <ColumnDefinition Width=".25*"></ColumnDefinition>
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Grid.Column="0" Margin="4,8,4,4" Text="Total Debit"></TextBlock>
                                                        <TextBox Grid.Column="1" Margin="4"   IsEnabled="False" x:Name="txtCompanyReceiptDr" HorizontalAlignment="Stretch" TextAlignment="Right" VerticalAlignment="Center" ></TextBox>
                                                        <TextBlock  Grid.Column="2" Margin="4,8,4,4" VerticalAlignment="Center"  HorizontalAlignment="Right" Text="Total Credit"></TextBlock>

                                                        <TextBox Grid.Column="3" Margin="4" IsEnabled="False" x:Name="txtCompanyReceiptCr" TextAlignment="Right" HorizontalAlignment="Stretch" VerticalAlignment="Center" ></TextBox>
                                                    </Grid>

                                                </Grid>


                                            </Grid>

                                        </controlsToolkit:DockPanel>
                                    </controls:TabItem>
                                    <controls:TabItem x:Name="tbAdvance" Style="{StaticResource TabItemStyle}"  Padding="0" Margin="0" Header="Advance" >
                                        <controlsToolkit:DockPanel LastChildFill="True" >
                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition ></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                </Grid.RowDefinitions>
                                                <data:DataGrid x:Name="dgAdvance" VerticalAlignment="Stretch"  Grid.Row="0" HorizontalAlignment="Stretch" ItemsSource="{Binding}" AutoGenerateColumns="False">
                                                    <data:DataGrid.Columns>
                                                        <data:DataGridTextColumn Header="Ledger Name" Width=".20*"  Binding="{Binding LedgerName, Mode=OneWay}" IsReadOnly="True" />

                                                        <data:DataGridTextColumn  Width=".20*" IsReadOnly="True" Binding="{Binding DR,StringFormat=0.000, Mode=TwoWay}" Header="Dr" >
