                                                    <inputToolkit:TimePicker x:Name="tpToTimeSunday"  Grid.Row="2" Grid.Column="6" Margin="4" HorizontalAlignment="Stretch" TabIndex="7" LostFocus="tpToTimeSunday_LostFocus" Width="100"/>
