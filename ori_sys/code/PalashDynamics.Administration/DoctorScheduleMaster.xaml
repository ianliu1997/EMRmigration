                                                    <inputToolkit:TimePicker x:Name="tpToTimeTueday"  Grid.Row="4" Grid.Column="6" Margin="4"  HorizontalAlignment="Stretch" TabIndex="17" LostFocus="tpToTimeTueday_LostFocus" Width="100"/>
                                                    <inputToolkit:TimePicker x:Name="tpToTimeWedday"  Grid.Row="5" Grid.Column="6" Margin="4"  HorizontalAlignment="Stretch" TabIndex="22" LostFocus="tpToTimeWedday_LostFocus" Width="100"/>
                                                    <inputToolkit:TimePicker x:Name="tpToTimeThrday"  Grid.Row="6" Grid.Column="6" Margin="4"  HorizontalAlignment="Stretch" TabIndex="26" LostFocus="tpToTimeThrday_LostFocus" Width="100"/>
