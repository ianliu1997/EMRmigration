                                                    <CheckBox x:Name="chkMon" Content="Monday" Margin="4" Grid.Row="3" Grid.Column="0"  HorizontalAlignment="Stretch"  Checked="chkMon_Checked" TabIndex="8" Unchecked="chkMon_Unchecked"></CheckBox>
                                                    <CheckBox x:Name="chkTue" Content="Tuesday" Margin="4" Grid.Row="4" Grid.Column="0"  HorizontalAlignment="Stretch" Checked="chkTue_Checked" TabIndex="13" Unchecked="chkTue_Unchecked"></CheckBox>
                                                    <CheckBox x:Name="chkWed" Content="Wednesday" Margin="4" Grid.Row="5" Grid.Column="0"  HorizontalAlignment="Stretch" Checked="chkWed_Checked" TabIndex="18" Unchecked="chkWed_Unchecked"></CheckBox>
