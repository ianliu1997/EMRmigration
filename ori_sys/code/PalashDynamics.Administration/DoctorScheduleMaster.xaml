                                                    <Border Grid.Column="1" Grid.Row="0" Grid.RowSpan="9" Width="1" >
                                                        <Rectangle Fill="Black" Width="1" ></Rectangle>
                                                    </Border>
                                                    <TextBlock Text="1st Schedule" TextAlignment="Left"  Margin="4" Grid.Row="0" Grid.Column="2" Grid.ColumnSpan="3"  HorizontalAlignment="Stretch"></TextBlock>

                                                    <TextBlock Text="From Time" Margin="4"  Grid.Row="1" Grid.Column="2" TextAlignment="Left"  HorizontalAlignment="Stretch"></TextBlock>
                                                    <inputToolkit:TimePicker x:Name="tpFromTimeSun"  Grid.Row="2" Grid.Column="2" Margin="4"  HorizontalAlignment="Stretch" TabIndex="4" LostFocus="tpFromTimeSun_LostFocus" Width="100"/>
