                                        <inputcombo:AutoCompleteComboBox  x:Name="cmbTimeMon"  Grid.Row="3" Grid.Column="4" Margin="4" VerticalAlignment="Center" Width="85" ItemsSource="{Binding}" HorizontalAlignment="Left"></inputcombo:AutoCompleteComboBox>
                                        <inputcombo:AutoCompleteComboBox  x:Name="cmbTimeTue"  Grid.Row="4" Grid.Column="4" Margin="4" VerticalAlignment="Center" Width="85" ItemsSource="{Binding}" HorizontalAlignment="Left"></inputcombo:AutoCompleteComboBox>
