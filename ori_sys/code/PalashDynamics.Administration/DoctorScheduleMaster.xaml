                                                    <Border Grid.Column="4" Grid.Row="0" Grid.RowSpan="9" Width="1">
                                                        <Rectangle Fill="Black" Width="1" ></Rectangle>
                                                    </Border>
                                                    <TextBlock Text="2nd Schedule" TextAlignment="Left"  Margin="4" Grid.Row="0" Grid.Column="5" Grid.ColumnSpan="3" HorizontalAlignment="Stretch"></TextBlock>

                                                    <TextBlock Text="From Time" Margin="4"  Grid.Row="1" Grid.Column="5" HorizontalAlignment="Stretch"></TextBlock>
                                                    <inputToolkit:TimePicker x:Name="tpFromTimeSunday"  Grid.Row="2" Grid.Column="5" Margin="4" HorizontalAlignment="Stretch" TabIndex="6" LostFocus="tpFromTimeSunday_LostFocus" Width="100"/>
