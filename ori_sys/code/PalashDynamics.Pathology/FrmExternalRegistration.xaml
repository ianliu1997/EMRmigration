                                                <inputcombo:AutoCompleteComboBox  Grid.Row="0" Grid.Column="1"   x:Name="cmbPatientCategory"  Margin="4,10,4,4"  VerticalAlignment="Center" SelectedItemBinding="{Binding clsPatientSourceVO, Mode=OneWay}" SelectedValuePath="ID" SelectionChanged="cmbPatientCategory_SelectionChanged" TabIndex="1"/>


