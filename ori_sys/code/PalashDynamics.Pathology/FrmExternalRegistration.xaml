                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition></RowDefinition>
                                                    <RowDefinition  Height="Auto"></RowDefinition>
                                                    <RowDefinition></RowDefinition>
                                                    <RowDefinition  Height="Auto"></RowDefinition>
                                                    <RowDefinition  Height="Auto"></RowDefinition>
                                                    <RowDefinition  Height="Auto"></RowDefinition>
                                                    <RowDefinition  Height="Auto"></RowDefinition>
                                                </Grid.RowDefinitions>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                    <ColumnDefinition Width="226"></ColumnDefinition>
                                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                    <ColumnDefinition ></ColumnDefinition>
                                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                    <ColumnDefinition ></ColumnDefinition>
                                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                                    <ColumnDefinition ></ColumnDefinition>
                                                </Grid.ColumnDefinitions>

                                                <StackPanel x:Name="stkselectpatient" HorizontalAlignment="Left" Orientation="Horizontal" Grid.Column="1" Grid.Row="0" Grid.ColumnSpan="2">
                                                    <RadioButton  VerticalAlignment="Center" HorizontalAlignment="Left" x:Name="chkNewPatient" Click="chkNewPatient_Click"  Content="New Patient" GroupName="YesNo" Margin="4" ></RadioButton>
                                                    <RadioButton  VerticalAlignment="Center" HorizontalAlignment="Left" x:Name="chkExPatient" Click="chkNewPatient_Click" Content="Existing Patient" GroupName="YesNo" Margin="4"></RadioButton>

                                                </StackPanel>


                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="MR No."   Grid.Row="0" Grid.Column="2" Margin="4" ></TextBlock>
                                                <TextBox Grid.Row="0" Grid.Column="3" Margin="4" Text="{Binding GeneralDetails.MRNo, Mode=TwoWay}"  x:Name="txtMRNo"  HorizontalAlignment="Stretch"  IsReadOnly="True"></TextBox>

                                                <Button x:Name="btnSearchCriteria" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Row="0" Grid.Column="4" ToolTipService.ToolTip="Find Patient" Click="btnSearchCriteria_Click"  >
                                                    <Button.Content>
                                                        <Image Source="/PalashDynamics;component/Icons/xmag.png" Margin="2" Width="20" ></Image>
                                                    </Button.Content>
                                                </Button>

                                                <TextBlock  VerticalAlignment="Center" HorizontalAlignment="Right" Text="Prefix" Margin="4" Grid.Row="1" Grid.Column="0"></TextBlock>
                                                <CustomCombo:AutoCompleteComboBox Grid.Row="1" Grid.Column="1" Margin="4" x:Name="cmbPreffix" VerticalAlignment="Center" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID" TabIndex="1" ></CustomCombo:AutoCompleteComboBox>


                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="First Name" Grid.Row="1" Grid.Column="2" Margin="4" ></TextBlock>
                                                <TextBox Grid.Row="1" Grid.Column="3" Margin="4" Text="{Binding GeneralDetails.FirstName, Mode=TwoWay}" LostFocus="txtFirstName_LostFocus" KeyDown="txtFirstName_KeyDown" TextChanged="txtFirstName_TextChanged" x:Name="txtFirstName"   MaxLength="50" TabIndex="2" ></TextBox>

                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Middle Name" Grid.Row="1" Grid.Column="4" Margin="4" ></TextBlock>
                                                <TextBox Grid.Row="1" Grid.Column="5" Margin="4" Text="{Binding GeneralDetails.MiddleName, Mode=TwoWay}" LostFocus="txtMiddleName_LostFocus" KeyDown="txtFirstName_KeyDown" TextChanged="txtFirstName_TextChanged"  x:Name="txtMiddleName"   MaxLength="50" TabIndex="3"></TextBox>

                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Last Name" Grid.Row="1" Grid.Column="6" Margin="4" ></TextBlock>
                                                <TextBox Grid.Row="1" Grid.Column="7" Margin="4" Text="{Binding GeneralDetails.LastName, Mode=TwoWay}" x:Name="txtLastName" LostFocus="txtLastName_LostFocus" KeyDown="txtFirstName_KeyDown" TextChanged="txtFirstName_TextChanged"   MaxLength="50" TabIndex="4"></TextBox>

                                                
                                               
                                                <!--<TextBlock  VerticalAlignment="Center" HorizontalAlignment="Right" Text="Blood Group" Margin="4" Grid.Row="2" Grid.Column="0"></TextBlock>
                                                <CustomCombo:AutoCompleteComboBox Grid.Row="2" Grid.Column="1" Margin="4" x:Name="cmbBloodGroup" VerticalAlignment="Center" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID" TabIndex="6" ></CustomCombo:AutoCompleteComboBox>-->

                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="D.O.B." Grid.Row="2" Grid.Column="0" Margin="4" ></TextBlock>
                                                <controls:DatePicker x:Name="dtpDOB" Grid.Row="2" Grid.Column="1" LostFocus="dtpDOB_LostFocus"  Margin="4" SelectedDate="{Binding GeneralDetails.DateOfBirth, Mode=TwoWay , NotifyOnValidationError=True, ValidatesOnExceptions=True }" Style="{StaticResource DatePickerStyle}"  TabIndex="5" />

                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Age" Grid.Row="2" Grid.Column="2" Margin="4" ></TextBlock>
                                                <Grid  Grid.Row="2" Grid.Column="3">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition />
                                                        <ColumnDefinition />
                                                        <ColumnDefinition />
                                                    </Grid.ColumnDefinitions>
                                                    <TextBox x:Name="txtYY"  Margin="4" Grid.Column="0"  HorizontalAlignment="Stretch" LostFocus="txtYY_LostFocus" TextChanged="txtNumber_TextChanged" KeyDown="txtNumber_KeyDown" TextAlignment="Right"  MaxLength="3" TabIndex="6"  />
                                                    <TextBox x:Name="txtMM"  Margin="4" Grid.Column="1"  HorizontalAlignment="Stretch" LostFocus="txtMM_LostFocus" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" TextAlignment="Right"  MaxLength="2" TabIndex="7" />
                                                    <TextBox x:Name="txtDD"  Margin="4" Grid.Column="2"  HorizontalAlignment="Stretch" LostFocus="txtDD_LostFocus" TextChanged="txtNumber_TextChanged"  KeyDown="txtNumber_KeyDown" TextAlignment="Right"  MaxLength="2" TabIndex="8" />
                                                </Grid>

                                                <!--<TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Education" Margin="4" Grid.Row="3" Grid.Column="0">
                                                </TextBlock>-->
