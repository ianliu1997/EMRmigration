                                                        <my:WaterMarkTextbox Margin="4" WaterMark="STD"  x:Name="txtResiSTD" KeyDown="txtResiSTD_KeyDown" OnTextChanged="txtResiSTD_OnTextChanged"  Grid.Column="1" TextBoxAlignment="Right"  TabIndex="12"/>
