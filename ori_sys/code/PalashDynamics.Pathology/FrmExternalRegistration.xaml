                                                <input:AutoCompleteBox Margin="4" Grid.Row="10" Grid.Column="1" Text="{Binding Taluka, Mode=TwoWay}"  x:Name="txtTaluka" Visibility="Collapsed"></input:AutoCompleteBox>

                                            </Grid>
