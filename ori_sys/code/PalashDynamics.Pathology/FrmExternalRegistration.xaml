                                                <inputcombo:AutoCompleteComboBox Grid.Column="3" Margin="4,10,4,4" VerticalAlignment="Center"  x:Name="cmbPatientSource"   SelectedValuePath="ID" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectionChanged="cmbPatientSource_SelectionChanged" TabIndex="2" Grid.ColumnSpan="2" />

                                                <TextBlock   VerticalAlignment="Center" HorizontalAlignment="Right" Grid.Column="5" Margin="4" Text="Company" />
                                                <inputcombo:AutoCompleteComboBox Grid.Column="6"   x:Name="cmbCompany"  Margin="3,2,3,2"  VerticalAlignment="Center" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectionChanged="cmbCompany_SelectionChanged"  SelectedValuePath="ID" TabIndex="3"/>

