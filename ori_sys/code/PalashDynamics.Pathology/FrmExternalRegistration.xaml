                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="District" Margin="8,4,0,4" Grid.Row="8" Grid.Column="0" Visibility="Collapsed"></TextBlock>
                                                <!--<CustomCombo:AutoCompleteComboBox Grid.Row="6" Grid.Column="1"   Margin="4" x:Name="cmbDistrict" VerticalAlignment="Center" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID"  SelectedValue="{Binding Address.CountryId,Mode=TwoWay}"   ></CustomCombo:AutoCompleteComboBox>-->
                                                <!--<TextBox  Margin="4" Grid.Row="6" Grid.Column="1" Text="{Binding District, Mode=TwoWay}" x:Name="txtDistrict"  VerticalAlignment="Center"></TextBox>-->
                                                <inputcombo:AutoCompleteComboBox Margin="4" Grid.Row="8" Grid.Column="1" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}"  x:Name="txtDistrict"   Visibility="Collapsed"></inputcombo:AutoCompleteComboBox>


                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Taluka" Margin="4" Grid.Row="10" Grid.Column="0" Visibility="Collapsed"></TextBlock>
                                                <!--<CustomCombo:AutoCompleteComboBox Grid.Row="6" Grid.Column="3"   Margin="4" x:Name="cmbTaluka" VerticalAlignment="Center" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID"   SelectedValue="{Binding Address.StateId,Mode=TwoWay}" ></CustomCombo:AutoCompleteComboBox>-->
