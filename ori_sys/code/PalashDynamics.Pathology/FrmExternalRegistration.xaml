                    <!--<ScrollViewer Background="Transparent" VerticalScrollBarVisibility="Auto" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" controlsToolkit:ScrollViewerExtensions.IsMouseWheelScrollingEnabled="True">-->
                    <Grid x:Name="pnlMain">

                        <Grid.RowDefinitions>
                            <RowDefinition ></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                        </Grid.RowDefinitions>

                        <controls:TabControl Padding="0"  BorderBrush="{StaticResource TabControlDarkerBorder}" x:Name="PatPersonalInfo" Margin="4,-4,4,4"  Grid.Column="0" Grid.Row="0" SelectionChanged="PatPersonalInfo_SelectionChanged" >
