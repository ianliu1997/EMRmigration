                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Marital Status" Margin="4" Grid.Row="2" Grid.Column="4"></TextBlock>
                                                <inputcombo:AutoCompleteComboBox Grid.Row="2" Grid.Column="5"   Margin="4"   x:Name="cmbMaritalStatus" VerticalAlignment="Center" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID" TabIndex="9" ></inputcombo:AutoCompleteComboBox>

                                               
                                                <!--<TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Occupation" Margin="4" Grid.Row="3" Grid.Column="4"></TextBlock>
                                                <inputcombo:AutoCompleteComboBox Grid.Row="3" Grid.Column="5"   Margin="4"   x:Name="cmbOccupation" VerticalAlignment="Center" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID" TabIndex="13" ></inputcombo:AutoCompleteComboBox>-->

                                                <!--collapsed by rohini as per disscussion with dr priyanka-->
                                                <TextBlock Visibility="Collapsed" VerticalAlignment="Center" HorizontalAlignment="Right" Text="Company Name" Margin="4" Grid.Row="3" Grid.Column="2"></TextBlock>
                                                <TextBox Visibility="Collapsed" Grid.Row="3" Grid.Column="3"   Margin="4"  Text="{Binding GeneralDetails.CompanyName, Mode=TwoWay}"    x:Name="txtOfficeName" VerticalAlignment="Center" MaxLength="100" TabIndex="14"></TextBox>

                                                  <Grid  Grid.Row="4" Grid.Column="5" Visibility="Collapsed">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition />
                                                        <ColumnDefinition />
                                                    </Grid.ColumnDefinitions>
                                                    <CustomCombo:AutoCompleteComboBox TabIndex="15"  Grid.Column="0" Margin="4"   x:Name="cmbIdentity"  VerticalAlignment="Center" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}"  SelectedValuePath="ID" ></CustomCombo:AutoCompleteComboBox>
                                                    <TextBox x:Name="txtIdentityNumber" Text="{Binding IdentityNumber, Mode=TwoWay}"  Margin="4" Grid.Column="1"  HorizontalAlignment="Stretch"   TextAlignment="Right"  />
                                                </Grid>

                                              
                                               
                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Gender" Grid.Row="2" Grid.Column="6" Margin="4" ></TextBlock>
                                                <CustomCombo:AutoCompleteComboBox  x:Name="cmbGender"  Background="Red" Grid.Row="2" Grid.Column="7"   Margin="4" VerticalAlignment="Center" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID"  TabIndex="10" ></CustomCombo:AutoCompleteComboBox>

                                              
                                            </Grid>
                                        </Grp:GroupBox>

