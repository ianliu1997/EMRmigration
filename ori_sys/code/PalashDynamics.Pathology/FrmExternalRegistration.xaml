                                                <TextBox  Margin="4" Grid.Row="5" Grid.Column="1" Grid.ColumnSpan="3" Text="{Binding AddressLine2, Mode=TwoWay}" x:Name="txtAddress2" MaxLength="150" Visibility="Collapsed"></TextBox>

                                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Line3" Margin="4" Grid.Row="6" Grid.Column="0" Visibility="Collapsed"></TextBlock>
