                                                        </Grid.ColumnDefinitions>
                                                        <my:WaterMarkTextbox Margin="4" WaterMark="Country" x:Name="txtMobileCountryCode" Grid.Column="0" TextBoxAlignment="Right" KeyDown="txtMobileCountryCode_KeyDown" OnTextChanged="txtMobileCountryCode_OnTextChanged"  TabIndex="14"></my:WaterMarkTextbox>
                                                        <my:WaterMarkTextbox Margin="4"   WaterMark="Mobile Number" x:Name="txtContactNo1" KeyDown="WaterMarkTextbox_KeyDown" OnTextChanged="WaterMarkTextbox_OnTextChanged"   Grid.Column="1" TextBoxAlignment="Right"  TabIndex="15" ></my:WaterMarkTextbox>
                                                      
                                                    </Grid>
                                                    <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" Text="Email" Margin="4" Grid.Row="0" Grid.Column="4"></TextBlock>
                                                         <TextBox x:Name="txtEmail" Margin="4" Grid.Row="0" Grid.Column="5"   Text="{Binding Email, Mode=TwoWay}"  MaxLength="100" TabIndex="16" ></TextBox>
                                                </Grid>
