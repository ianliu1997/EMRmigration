<controls:ChildWindow x:Class="PalashDynamics.Pharmacy.ItemStore_new"
           xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"  xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"                       
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
           xmlns:controls="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls"
           xmlns:Localization="clr-namespace:PalashDynamic.Localization;assembly=PalashDynamic.Localization"
           Width="1150" Height="500"  Loaded="ChildWindow_Loaded"
           xmlns:Grp="clr-namespace:Groupbox;assembly=Groupbox"
           xmlns:data="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Data"   
           xmlns:inputcombo="clr-namespace:PalashDynamics.Controls;assembly=PalashDynamics.Controls.AutoCompleteComboBox"
           xmlns:controlsToolkit="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Toolkit"            
           Title="Item Store" >

    <controls:ChildWindow.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/PalashDynamics;component/Generic.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <Localization:Localized x:Key="locResource"></Localization:Localized>
        </ResourceDictionary>
    </controls:ChildWindow.Resources>
    <Grid>
        <toolkit:DockPanel x:Name="MainDock" LastChildFill="True" toolkit:DockPanel.Dock="Top">
            <ScrollViewer Grid.Row="1" Background="Transparent" VerticalScrollBarVisibility="Auto"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" controlsToolkit:ScrollViewerExtensions.IsMouseWheelScrollingEnabled="True">
                <Border BorderBrush="{StaticResource TabControlDarkerBorder}"   BorderThickness="1" CornerRadius="3,3,0,0" Margin="4,4,4,4"  Background="Transparent">
                    <toolkit:DockPanel toolkit:DockPanel.Dock="Top" LastChildFill="True">
                        <toolkit:DockPanel toolkit:DockPanel.Dock="Bottom" Margin="4">
                            <TextBlock Text=" Pleae Enter All Quantities In Stocking UOM" Foreground="Red" FontWeight="Bold" VerticalAlignment="Center" ></TextBlock>
                            <StackPanel x:Name="FooterBackPanel" Orientation="Horizontal" Margin="0,3,5,3" HorizontalAlignment="Right" Background="Transparent">
                                <StackPanel x:Name="SaveCommandPanelBackPanel" Orientation="Horizontal"  Margin="0,0,0,0">
                                    <ToggleButton x:Name="OKButton" Click="OKButton_Click"  Height="25" Content="Apply Store" Width="Auto"  Background="#000000" Margin="5,0,445,0" HorizontalAlignment="Stretch" Style="{StaticResource ToggleButtonStyle}" Visibility="Visible"  />
                                    <ToggleButton x:Name="cmdAddTax"  Content="Add Tax"  Height="25" Background="#000000" Margin="4" HorizontalAlignment="Right" Style="{StaticResource ToggleButtonStyle}" Visibility="Visible" Click="cmdAddTax_Click" />
                                    <ToggleButton x:Name="CloseButton" Click="CancelButton_Click"  Height="25" Content="{Binding btnClose,Source={StaticResource locResource}}"  Background="#000000" Margin="5,0,0,0" HorizontalAlignment="Stretch" Style="{StaticResource ToggleButtonStyle}" Visibility="Visible"   />
                                </StackPanel>
                            </StackPanel>
                            <!--{Binding btnSave,Source={StaticResource locResource}}-->
                        </toolkit:DockPanel>
                        <toolkit:DockPanel toolkit:DockPanel.Dock="Top" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="4">
                            <Grid Width="Auto">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="auto"></RowDefinition>
                                    <RowDefinition Height="auto"></RowDefinition>
                                    <RowDefinition Height="auto"></RowDefinition>
                                    <RowDefinition Height="auto"></RowDefinition>
                                    <RowDefinition ></RowDefinition>
                                    <RowDefinition ></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" ></ColumnDefinition>
                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid x:Name="grdItemName" Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Text="Item" Grid.Column="0" Grid.Row="0" Margin="4" Visibility="Collapsed"></TextBlock>
                                    <TextBlock x:Name="lblItemName" VerticalAlignment="Center" HorizontalAlignment="Stretch" Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="0" Margin="4" AllowDrop="False" TextWrapping="Wrap"></TextBlock>
                                </Grid>
                                <Grid x:Name="grdClinic" Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="7" Grid.RowSpan="1">
                                    <Grid.RowDefinitions>
                                        <RowDefinition></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                                        <ColumnDefinition></ColumnDefinition>
                                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Text="Clinic" HorizontalAlignment="Right" VerticalAlignment="Center" Grid.Row="0" Grid.Column="0" Width="Auto"></TextBlock>
                                    <inputcombo:AutoCompleteComboBox x:Name="cmbClinic" TabIndex="4" SelectedItemBinding="{Binding MasterListItem, Mode=OneWay}" SelectedValuePath="ID" SelectionChanged="cmbClinic_SelectionChanged" Grid.Row="0" Grid.Column="1"  Margin="4" HorizontalAlignment="Stretch"></inputcombo:AutoCompleteComboBox>
                                    <Button x:Name="cmdSearch" Margin="4,0,4,4" Height="25" TabIndex="3"  Grid.Column="4"  ToolTipService.ToolTip="Search Clinic" Click="CmdSearch_Click" >
                                        <Image  Source="/PalashDynamics;component/Icons/Search_b.JPG" Margin="4,0,4,4" Width="20" ></Image>
                                    </Button>
                                </Grid>

                                <ScrollViewer  x:Name="Viewerstore" Grid.Row="4" Grid.Column="0" Grid.RowSpan="4" Margin="4" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" MouseWheel="ViewerStore_MouseWheel">
                                    <Grid Width="500"  Grid.Row="4" Grid.Column="2" Grid.RowSpan="4" Grid.ColumnSpan="2"  >
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="auto"></RowDefinition>
                                            <RowDefinition  Height="auto"></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <TextBlock Text="{Binding lblStore,Source={StaticResource locResource}}" VerticalAlignment="Center" HorizontalAlignment="Stretch" Grid.Column="0" Grid.Row="0" Margin="4"></TextBlock>
                                        <CheckBox x:Name="chkAllStores" Margin="82,4,47,3" Content="Select All" IsChecked="{Binding CheckedAllTariffs,Mode=TwoWay}" Grid.Column="1" Grid.Row="0" Click="chkAllStores_Click"></CheckBox>
                                        <data:DataGrid x:Name="dgStoreList" Height="280" AutoGenerateColumns="False"  ItemsSource="{Binding}" VerticalScrollBarVisibility="Visible" HorizontalScrollBarVisibility="Visible" IsReadOnly="True" Margin="4" Grid.Row="1" TabIndex="23" SelectionChanged="dgStoreList_SelectionChanged">
                                            <data:DataGrid.Columns>
                                                <data:DataGridTemplateColumn Header="Select" Width="Auto" Visibility="Visible">
                                                    <data:DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <CheckBox x:Name="chkStore"  IsChecked="{Binding status, Mode=TwoWay}"  VerticalAlignment="Center" HorizontalAlignment="Center" Width="Auto" Click="chkStore_Click" ></CheckBox>
                                                        </DataTemplate>
                                                    </data:DataGridTemplateColumn.CellTemplate>
                                                </data:DataGridTemplateColumn>
                                                <data:DataGridTextColumn Binding="{Binding StoreName}" Header="Store Name" Width="Auto"></data:DataGridTextColumn>
                                                <data:DataGridTemplateColumn Header="Minimum" Width="Auto" >
                                                    <data:DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <TextBox x:Name="txtMin" Text="{Binding Min,Mode=TwoWay}" Width="50" MaxLength="5" Margin="4" VerticalAlignment="Center" TextChanged="txtExpirationdays_TextChanged" KeyDown="txtNumber_KeyDown1"></TextBox>
                                                        </DataTemplate>
                                                    </data:DataGridTemplateColumn.CellTemplate>
                                                </data:DataGridTemplateColumn>

                                                <data:DataGridTemplateColumn Header="Maximum" Width="Auto">
                                                    <data:DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <TextBox x:Name="txtMax" Text="{Binding Max,Mode=TwoWay}" Width="50" MaxLength="5" Margin="4" VerticalAlignment="Center" TextChanged="txtExpirationdays_TextChanged" KeyDown="txtNumber_KeyDown1"></TextBox>
                                                        </DataTemplate>
                                                    </data:DataGridTemplateColumn.CellTemplate>
                                                </data:DataGridTemplateColumn>

                                                <data:DataGridTemplateColumn Header="ReOrder" Width="Auto" >
                                                    <data:DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <TextBox x:Name="txtReorder" Text="{Binding Re_Order,Mode=TwoWay}" Width="50" MaxLength="5" Margin="4" VerticalAlignment="Center" TextChanged="txtExpirationdays_TextChanged" KeyDown="txtNumber_KeyDown1" ></TextBox>
                                                            <!--KeyDown="txtNumber_KeyDown"-->
                                                        </DataTemplate>
                                                    </data:DataGridTemplateColumn.CellTemplate>
                                                </data:DataGridTemplateColumn>
                                            </data:DataGrid.Columns>
                                        </data:DataGrid>
                                    </Grid>
                                </ScrollViewer>
                                <Border Width="Auto" BorderBrush="{StaticResource TabControlDarkerBorder}"   Grid.Row="4" Grid.Column="4" Grid.RowSpan="4" Grid.ColumnSpan="5" Margin="4" BorderThickness="1" CornerRadius="3,3,0,0" Background="Transparent">
                                    <ScrollViewer x:Name="ViewerTax" Margin="1" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" MouseWheel="ViewerStore_MouseWheel">
                                        <Grid >
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="auto"></RowDefinition>
                                                <RowDefinition Height="*"></RowDefinition>
                                                <RowDefinition Height="auto" ></RowDefinition>
                                            </Grid.RowDefinitions>
                                            <TextBlock Text="Taxes" VerticalAlignment="Center" HorizontalAlignment="Stretch" Grid.Column="0" Grid.Row="1" Margin="4" Visibility="Collapsed"></TextBlock>


                                            <data:DataGrid Grid.Row="1"  x:Name="dgTaxList" AutoGenerateColumns="False" ItemsSource="{Binding}" VerticalScrollBarVisibility="Visible" HorizontalScrollBarVisibility="Visible" IsReadOnly="True" Margin="4" HorizontalAlignment="Stretch"  TabIndex="23" Width="550" SelectionChanged="dgTaxList_SelectionChanged">
                                                <data:DataGrid.Columns>
                                                        <data:DataGridTemplateColumn Header="View">
                                                            <data:DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <HyperlinkButton Margin="4" x:Name="cmdView" Content="View" Click="cmdView_Click" Cursor="Hand"></HyperlinkButton>
                                                                </DataTemplate>
                                                            </data:DataGridTemplateColumn.CellTemplate>
                                                        </data:DataGridTemplateColumn>

                                                        <data:DataGridTextColumn Binding="{Binding TaxName}" Width="Auto" Header="TaxName" ></data:DataGridTextColumn>

                                                        <data:DataGridTemplateColumn Header="Applicable For" IsReadOnly="False" Width="Auto">
                                                            <data:DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <TextBox Text="{Binding ApplicableForDesc,StringFormat=0.00, Mode=TwoWay}" IsEnabled="False" Tag="{Binding }" MaxWidth="100" MaxLength="13"  Margin="4" ></TextBox>
                                                                </DataTemplate>
                                                            </data:DataGridTemplateColumn.CellTemplate>
                                                        </data:DataGridTemplateColumn>
                                                        <data:DataGridTemplateColumn Header="Applicable On" IsReadOnly="False" Width="Auto">
                                                            <data:DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <TextBox Text="{Binding ApplicableOnDesc,StringFormat=0.00, Mode=TwoWay}" IsEnabled="False"  Tag="{Binding }" MaxWidth="100" MaxLength="13"  Margin="4" ></TextBox>
                                                                </DataTemplate>
                                                            </data:DataGridTemplateColumn.CellTemplate>
                                                        </data:DataGridTemplateColumn>

                                                        <data:DataGridTemplateColumn Header="Percentage" IsReadOnly="False" Width="Auto">
                                                            <data:DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <TextBox Text="{Binding Percentage,StringFormat=0.00, Mode=TwoWay}" IsEnabled="False" Tag="{Binding }" MaxWidth="100" MaxLength="5"  Margin="4" ></TextBox>
                                                                </DataTemplate>
                                                            </data:DataGridTemplateColumn.CellTemplate>
                                                        </data:DataGridTemplateColumn>

                                                        <data:DataGridTextColumn Binding="{Binding TaxTypeName}" Width="Auto" Header="Tax Type" ></data:DataGridTextColumn>
                                                    <data:DataGridTextColumn Binding="{Binding ApplicableFrom,StringFormat=d,Mode=OneWay,ConverterParameter={StaticResource GridDateDisplayFormat}}" Visibility="Collapsed" Header="Applicable From" IsReadOnly="True" />
                                                    <data:DataGridTextColumn Binding="{Binding ApplicableTo,StringFormat=d,Mode=OneWay,ConverterParameter={StaticResource GridDateDisplayFormat}}" Visibility="Collapsed"  Header="Applicable To" IsReadOnly="True" />
                                                    <data:DataGridTemplateColumn Header="GST" IsReadOnly="True" Visibility="Collapsed" >
                                                            <data:DataGridTemplateColumn.CellTemplate >
                                                                <DataTemplate >
                                                                    <CheckBox x:Name="chkIsGST" Margin="10,2,10,2" IsChecked="{Binding IsGST,Mode=TwoWay}" IsEnabled="False" VerticalAlignment="Center" HorizontalAlignment="Center" ></CheckBox >
                                                                </DataTemplate>
                                                            </data:DataGridTemplateColumn.CellTemplate>
                                                        </data:DataGridTemplateColumn>

                                                        <data:DataGridTemplateColumn Header="Delete">
                                                            <data:DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <HyperlinkButton VerticalAlignment="Center" FontFamily="Webdings" Content="r"  Padding="5,0,5,0" IsTabStop="False" Click="cmdDeleteItem_Click"  x:Name="cmdDeleteItem" ToolTipService.ToolTip="Delete" Visibility="Visible"></HyperlinkButton>
                                                                </DataTemplate>
                                                            </data:DataGridTemplateColumn.CellTemplate>
                                                        </data:DataGridTemplateColumn>
                                                        <sdk:DataGridTextColumn IsReadOnly="True" Visibility="Collapsed" Binding="{Binding ID}" />
                                                    </data:DataGrid.Columns>
                                            </data:DataGrid>

                                            <Grp:GroupBox x:Name="grpAddTax" Grid.Row="2" Grid.ColumnSpan="8" Header="Add Tax" Width="550" >
                                                <ScrollViewer x:Name="AddTaxViewer" Margin="1" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" MouseWheel="ViewerStore_MouseWheel">

                                                    <Grid x:Name="grdAddTaxItem" Grid.Row="2" >
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="98*"></ColumnDefinition>
                                                            <ColumnDefinition Width="98*"></ColumnDefinition>
                                                            <ColumnDefinition Width="98*" />
                                                            <ColumnDefinition Width="98*"></ColumnDefinition>
                                                            <ColumnDefinition Width="80*"></ColumnDefinition>
                                                            <!--<ColumnDefinition Width="80*"></ColumnDefinition>-->
                                                            <ColumnDefinition Width="80*"></ColumnDefinition>
                                                        </Grid.ColumnDefinitions>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                        </Grid.RowDefinitions>
                                                        <TextBlock Text="Store" Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="4,0,0,0" Grid.Column="0" Width="Auto"></TextBlock>
                                                        <inputcombo:AutoCompleteComboBox Grid.Row="1" Grid.Column="0" x:Name="cmbStore" SelectedValue="{Binding clsItemStoreVO.StoreID}" SelectedItem="{Binding clsItemStoreVO,Mode=TwoWay}"  ItemsSource="{Binding SelectedStoreList}" Margin="4" HorizontalAlignment="Stretch" SelectionChanged="cmbStore_SelectionChanged"/>
                                                        <TextBlock Text="Tax" Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="4,0,0,0" Grid.Column="1" Width="Auto"></TextBlock>
                                                        <inputcombo:AutoCompleteComboBox Grid.Row="1" Grid.Column="1" x:Name="cmbTax" SelectedValue="{Binding ApplicableFor.ID}" SelectedItem="{Binding ApplicableFor,Mode=TwoWay}"  ItemsSource="{Binding ApplicableForList}" Margin="4" HorizontalAlignment="Stretch"/>
                                                        <TextBlock Text="Applicable For" Grid.Row="0" Grid.Column="2" HorizontalAlignment="Left" Margin="4,0,0,0"  VerticalAlignment="Center" Width="Auto"></TextBlock>
                                                        <inputcombo:AutoCompleteComboBox Grid.Row="1" Grid.Column="2" x:Name="cmbApplicableFor" SelectedValue="{Binding ApplicableFor.ID}" SelectedItem="{Binding ApplicableFor,Mode=TwoWay}"  ItemsSource="{Binding ApplicableForList}" Margin="4" HorizontalAlignment="Stretch"/>
                                                        <TextBlock Text="Applicable On" Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="4,0,0,0" Grid.Column="3" Width="Auto"></TextBlock>
                                                        <inputcombo:AutoCompleteComboBox Grid.Row="1" Grid.Column="3" x:Name="cmbApplicableOn" SelectedValue="{Binding ApplicableFor.ID}" SelectedItem="{Binding ApplicableFor,Mode=TwoWay}"  ItemsSource="{Binding ApplicableForList}" Margin="4" HorizontalAlignment="Stretch"/>
                                                        <TextBlock Text="Percentage" Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="4,0,0,0" Grid.Column="4" Width="Auto"></TextBlock>
                                                        <TextBox x:Name="txtPercentage" Text="{Binding Percentage,StringFormat=0.00, Mode=TwoWay}"  MaxLength="5" Grid.Row="1" TextAlignment="Right" Margin="4" Grid.Column="4" KeyDown="txtNumber_KeyDown"></TextBox>
                                                        <RadioButton GroupName="option" Name="taxseletion" Content="Inclusive" Grid.Column="5" Margin="1,8,5,24" Grid.RowSpan="2" Grid.ColumnSpan="2" />
                                                        <RadioButton GroupName="option" Name="taxseletion1" Content="Exclusive" Grid.Column="5" Margin="1,14,5,1" Grid.Row="1" IsChecked="True" Grid.ColumnSpan="2" />
                                                        <!--<ToggleButton x:Name="cmdAddTax"  Content="Add Tax" Grid.Row="1" Grid.Column="8" Height="20" Background="#000000" Margin="4" HorizontalAlignment="Right" Style="{StaticResource ToggleButtonStyle}" Visibility="Visible" Click="cmdAddTax_Click" />-->

                                                        <CheckBox x:Name="chkTaxForAllStores" Visibility="Collapsed" Grid.Row="2" Grid.Column="0" Margin="4" Content="Set To All stores" IsChecked="{Binding CheckedAllTariffs,Mode=TwoWay}" ></CheckBox>
                                                        <TextBlock Text="Applicable From" Grid.Row="3" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="4,0,0,0" Grid.Column="0" Width="Auto" Visibility="Collapsed"></TextBlock>
                                                        <controls:DatePicker x:Name="dtpApplicableFromDate" Grid.Row="4"  Grid.Column="0" Margin="4" HorizontalAlignment="Stretch" VerticalAlignment="Center" Visibility="Collapsed"  Text="{Binding StringFormat='{}{0:dd/MM/yyyy}'}"></controls:DatePicker>
                                                        <TextBlock Text="Applicable To" Grid.Row="3" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="4,0,0,0" Grid.Column="1" Width="Auto" Visibility="Collapsed" ></TextBlock>
                                                        <controls:DatePicker x:Name="dtpApplicableToDate" Grid.Row="4"  Grid.Column="1" Margin="4" HorizontalAlignment="Stretch" VerticalAlignment="Center" Visibility="Collapsed" Text="{Binding StringFormat='{}{0:dd/MM/yyyy}'}" ></controls:DatePicker>
                                                        <TextBlock Text="Is GST" Grid.Row="3" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="20,0,0,0" Grid.Column="2" Width="Auto" Visibility="Collapsed" ></TextBlock>
                                                        <CheckBox x:Name="chkIsGST" Margin="30,10,0,0"  Grid.Column="2" Grid.Row="4" Visibility="Collapsed" ></CheckBox>
                                                    </Grid>
                                                </ScrollViewer>
                                            </Grp:GroupBox>
                                            <!--<ToggleButton x:Name="cmdApplyTax" Click="cmdApplyTax_Click"  Content="Apply Tax" Grid.Row="4" Height="20" Background="#000000" Margin="4" HorizontalAlignment="Right" Style="{StaticResource ToggleButtonStyle}" Visibility="Visible"  />-->
                                        </Grid>
                                    </ScrollViewer>
                                </Border>
                            </Grid>
                        </toolkit:DockPanel>
                    </toolkit:DockPanel>
                </Border>
            </ScrollViewer>
        </toolkit:DockPanel>
    </Grid>
</controls:ChildWindow>